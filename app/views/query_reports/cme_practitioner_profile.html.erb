<div class="enc-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-3">
                   <%= render partial: "verification_platform/encompass_sidebar"%>
                </div>
                <div class="col-xl-9">
                    <%= render partial: "verification_platform/encompass_nav"%>
                    <%= render "query_report_top_nav"%>
                    <form action="">
                        <div class="form-row row mt-3">
                            <h6 class="form-heading">Search by Date Range</h6>
                            <div class="col-xl-6">
                                <label for="date-from">Date From:</label>
                                <input type="date" name="date-from" id="date-from" class="form-control py-1">
                            </div>
                            <div class="col-xl-6">
                                <label for="date-to">Date To:</label>
                                <input type="date" name="date-to" id="date-to" class="form-control py-1">
                            </div>
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6 d-flex align-items-end">
                                <div class=" form-check d-flex align-items-center gap-2">
                                    <input class="form-check-input" type="checkbox" id="include-guest-check"
                                        value="Include Guest">
                                    <label class="form-check-label" for="include-guest-check">Include Guest</label>
                                </div>
                            </div>
                            <div class="col-xl-6 d-flex align-items-end justify-content-end">
                                <div class="d-flex gap-2">
                                    <button type="button" class="btn btn-primary py-1">
                                        Filter
                                    </button>
                                    <button type="button" class="btn btn-primary py-1">
                                        Show All
                                    </button>
                                </div>
                            </div>
                        </div>
                    </form>
                    <table class="table practice-table table-bordered mt-3">
                        <thead>
                            <tr>
                                <th class="text-white" scope="col">ID #</th>
                                <th class="text-white">Practitioner Name</th>
                                <th class="text-white">Select Practitioner to Print</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>1</td>
                                <td>Lorem ipsum</td>
                                <td>Lorem ipsum</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>