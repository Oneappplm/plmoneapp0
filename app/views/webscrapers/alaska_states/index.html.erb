<div class="content container-fluid">
	<div class="row mb-3">
		<%= render partial: 'webscrapers/links' %>
		<div class="col-lg-12">
			<h2>
				State of Alaska
			</h2>
			<p>Source: <code>https://www.commerce.alaska.gov/cbp/main/Search/Professional</code></p>
		</div>
	</div>
	<%= form_with url: request.url, method: :get, html: { class: 'form_search_with_icon'} do |form| %>
	<div class="row mb-3">
			<div class="col-lg-3">
				<label for="">License Number</label>
				<input type="text" name="license_number" value="<%= params[:license_number] %>" class="form-control">
			</div>
			<!-- <div class="col-lg-3">
				<label for="">Program</label>
				<input type="text" name="program" value="<%= params[:program] %>" class="form-control">
			</div>
			<div class="col-lg-3">
				<label for="">License Type</label>
				<input type="text" name="license_type" value="<%= params[:license_type] %>" class="form-control">
			</div> -->
			<div class="col-lg-3">
				<br>
				<button class="btn btn-primary" type="submit">
						Search
				</button>
			</div>
	</div>
	<% end %>
	<br>
	<div class="row">
		<div class="col-lg-12">
			<table class="table">
					<thead>
								<tr>
												<th scope="col" class="bg-primary text-white">Program</th>
												<th scope="col" class="bg-primary text-white">License Number</th>
												<th scope="col" class="bg-primary text-white">DBA</th>
												<th scope="col" class="bg-primary text-white">Owners</th>
												<th scope="col" class="bg-primary text-white">Status</th>
												<th scope="col" class="bg-primary text-white">License Expiration</th>
								</tr>
				</thead>
				<tbody>
					<%= render partial: 'alaska_state', collection: @alaska_states, as: :item %>
				</tbody>
			</table>
		</div>
	</div>
</div>