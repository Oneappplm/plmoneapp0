<div class="content container-fluid">
	<div class="row mb-3">
		<div class="row mb-3">
			<div class="col-lg-12 d-flex justify-content-end gap-2">
				<%= link_to 'Autoverify', auto_verifies_path, class: "btn btn-primary"%>
			</div>
		</div>
		<div class="col-lg-12">
			<h2>
				Logs
			</h2>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12">
			<table class="table">
				<thead>
					<tr>
						<th scope="col" class="bg-primary text-white">Type</th>
						<th scope="col" class="bg-primary text-white">File Name</th>
						<th scope="col" class="bg-primary text-white">File Type</th>
						<th scope="col" class="bg-primary text-white">Status</th>
						<th scope="col" class="bg-primary text-white">Created Date</th>
					</tr>
				</thead>
				<tbody>
					<%= render partial: 'log', collection: @logs, as: :item %>
				</tbody>
			</table>
			</div>
	</div>
</div>