
    <div class="enc-content">
        <div class="container">
        <%= render "top_nav_buttons"%>
          
          
            <div class="license-content">
                <div class="container">
                    <div class="row">
                        <div class="mt-4 d-flex align-items-end justify-content-between gap-3">
                            <div class="d-flex align-items-center gap-3">
                                <h4 class="page-heading">Training Record 1</h4>
            
                                <button class="btn-record" type="submit">
                                    Edit
                                </button>
            
                            </div>
                        </div>
                        <div class="enc-tabs-wrp mt-2">
                            <div class="">
                            <button class="btn-record" name ="ver-letter" type="submit">
                                    Verification Letter
                                </button>
                            </div>
                            <div class="">
                                <button class="btn-record" name ="send-contract"  type="submit">
                                    Send Contract
                                </button>
                            </div>
                            <div class="">
                                <button class="btn-record" name ="view-scan-img" type="submit">
                                    View Scanned Images
                                </button>
                            </div>
                            <div class="col  d-flex justify-content-end align-items-center">
                            <button id ="btn-delete-record" class="btn btn-primary" type="button" style="backgorund-color:red;">
                            <i class="bi bi-trash"></i>
                            Delete Record
                            </button>
                         </div>
                    
            
                        </div>
            <div class="mt-3 row page-applicable">
                <div class="col-xl-5">
                    <div class="left-pane">
                        <h6 class="rva-license">RVA Information</h6>
            
                        <div class="enc-tabs-wrp mt-2">
                            <div class="">
                                <button class="rva-btn" name ="btn-request" type="submit">
                                    Request
                                </button>
                            </div>
                            <div class="">
                                <button class="rva-btn" name ="btn-ver" type="submit">
                                    Verification
                                </button>
                            </div>
                            <div class="">
                                <button class="rva-btn" name ="btn-audit" type="submit">
                                    Audit
                                </button>
                            </div>
            
                        </div>
                        <div class="enc-tabs-wrp mt-2">
                            <div class="">
                                <button class="rva-btn2" name ="btn-audit" type="submit">
                                    Quality Audit
                                </button>
                            </div>
                            <div class="">
                                <button class="rva-btn2" name ="btn-skip-rva" type="submit">
                                    Skip RVA
                                </button>
                            </div>
            
                        </div>
                    </div>
                </div>
                <div class="col-xl-7">
                    <div class="right-pane">
                        <div class="d-flex align-items-center justify-content-between">
                            <h6 class="msg-board">Message Board</h6>
                            <button name="lic-add-com" class="btn-record" data-bs-toggle="modal"
                                data-bs-target="#addCommentModal"  >
                                Add Comment
                            </button>
                         </div>
                        <div class="form-row mt-2">
                            <span class="span-pane" data-bs-toggle="modal" data-bs-target="#deleteCommentModal">
                                Subject: Incomplete Record
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            </div>
         
            
            
            </div>
                    </div>
                    
                    <form action="">
                                
                        <div class="form-row row mt-3">
                            <div class="col-xl-12 right-pane">
                                <div class="form-row row ">
                                    <div class="d-flex align-items-center gap-10">
                                        <span class="page-note">
                                            <i class='bx bx-error-circle font-blue'></i> <span>To search for an Institution/Program name, please click the search button.</span>
                                        </span>
                                    </div>
                                    <div class="col-xl-5">
                                        <label for="prog-name">Institution/Program Name:</label>
                                        <select name="prog-name" id="prog-name" class="form-select">
                                        <option value="">Select option</option>
                                        </select>
                            
                                    </div>
                                    <div class="col-xl-1">
                                        <button class="btn-search_add_training"  type="submit">
                                            Search
                                        </button>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="address">Address:</label>
                                        <select name="address" id="address" class="form-select">
                                        <option value="">Select option</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-6">
                                        <label for="contact">Contact:</label>
                                        <input type="text" name="contact" id="contact"  placeholder="Type here..." class="form-control" required>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="add-address">Additional Address:</label>
                                        <input type="text" name="add-address" id="add-address"  placeholder="Type here..." class="form-control" required>
                                    </div>
                                </div>
                                
                              
                                <div class="form-row row mt-4">
                                    <div class="col-xl-6">
                                        <label for="city">City:</label>
                                        <select name="city" id="city" class="form-select">
                                        <option value="">Select option</option>
                                        </select>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="county">County:</label>
                                        <input type="text" name="county" id="county"  placeholder="Type here..." class="form-control" required>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-4">
                                        <label for="state">State/Province:</label>
                                        <select name="state" id="state" class="form-select">
                                        <option value="">Select option</option>
                                        </select>
                                    </div>
                                    <div class="col-xl-4">
                                        <label for="zip">Zip/Postal Code:</label>
                                        <input type="text" id="lastname"  placeholder="Type here..." class="form-control" required>
                                    </div>
                                    <div class="col-xl-4">
                                        <label for="country">Country:</label>
                                        <select id="country"  name="country"  class="form-control" >
                                        <option value="">Select option</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-4">
                                        <label for="phone-number">Phone Number:</label>
                                        <input type="text" id="phone-number" name="phone-number" placeholder="Type here..." class="form-control" required>
                                    </div>
                                    <div class="col-xl-4">
                                        <label for="email-address">Email Address :</label>
                                        <input type="text" name="email-address" id="email-address" placeholder="Type here..." class="form-control" required>
                                    </div>
                                    <div class="col-xl-4">
                                        <label for="fax-number">Fax Number:</label>
                                        <input type="text" id="fax-number" name="fax-number" placeholder="Type here..." class="form-control" required>
                                    </div>
                                </div>
                               
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-4">
                                    <label for="type">Type of Training:</label>
                                    <select name="type" id="type" class="form-select">
                                    <option value="">Select option</option>
                                    </select>
                                </div>
                                <div class="col-xl-4">
                                    <label for="prog-dir-firstname">Program Director First Name:</label>
                                    <input type="text" name ="prog-dir-firstname"  placeholder="Type here..."id="prog-dir-firstname" class="form-control" required>
                                </div>
                                <div class="col-xl-4">
                                    <label for="prog-dir-firstname">Program Director Last Name:</label>
                                    <input type="text" name ="prog-dir-firstname" placeholder="Type here..." id="prog-dir-firstname" class="form-control" required>
                                </div>
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-4">
                                    <label for="prog-dir-type">Program Director Practitioner Type:</label>
                                    <select name="prog-dir-type" id="prog-dir-type" class="form-select">
                                     <option value="">Select option</option>
                                     </select>
                                </div>
                                
                            </div>
                        </div>
                        
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="d-flex align-items-center gap-3">
                                    <label for="" class="training-curProgdir">Is this the current Program Director?</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cur-prog" id="inter-yes" value="Yes" checked>
                                        <label class="form-check-label" for="prac-yes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="cur-prog" id="inter-no" value="No">
                                        <label class="form-check-label" for="prac-no">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                        <div class="form-row row mt-2">
                            
                            <div class="col-xl-4">
                                <label for="cur-prog-dir">If No, list current Program Director:</label>
                                <input type="text" name ="cur-prog-dir" id="cur-prog-dir" placeholder="Type here..." class="form-control" required>
                            </div>
                            
                        </div>
                        <div class="form-row row mt-2">
                            
                            <div class="col-xl-4">
                                <label for="specialty">Specialty:</label>
                                <select name="specialty" id="specialty" class="form-select">
                                <option value="">Select option</option>
                                </select>
                            </div>
                            
                        </div>
                        <div class="form-row row mt-2">
                            
                            <div class="col-xl-2">
                                <label for="from-date">Dates Attended:</label>
                                <input type="date" name="from-date" id="from-date" class="form-control">
                            </div>
                            
                            <div class="col-xl-2">
                                <label for="to-date"></label>
                                <input type="date"  name="to-date" id="to-date" class="form-control">
                            </div>
                            
                        </div>
                       
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="d-flex align-items-center gap-3">
                                    <label for="" class="training-curProgdir">Completed?</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="completed" id="inter-yes" value="Yes" checked>
                                        <label class="form-check-label" for="prac-yes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="completed" id="inter-no" value="No">
                                        <label class="form-check-label" for="prac-no">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                        <div class="form-row row mt-2">
                            
                            <div class="col-xl-4">
                                <label for="explain-no">If No, please explain:</label>
                                <input type="text" name ="explain-no" id="explain-no" placeholder="Type here..." class="form-control" required>
                            </div>
                            
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="d-flex align-items-center gap-3">
                                    <label for="" class="training-curProgdir">Show on Tickler?</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="tickler" id="inter-yes" value="Yes" checked>
                                        <label class="form-check-label" for="prac-yes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="tickler" id="inter-no" value="No">
                                        <label class="form-check-label" for="prac-no">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>
                        
                        </div>
                        <div class="form-row row mt-2">
                            
                            <div class="col-xl-25">
                                <label for="training-comment">Please List any comments</label>
                                <input type="text" id="training-comment" placeholder="Type here..." class="form-control" required>
                            </div>
                            
                        </div>
                        
                        <div class="form-row mt-1 d-flex justify-content-end">
                            <button class=" btn-license-save" type="submit">
                                 Save 
                            </button>
                        </div>
                        
                       
                               
                    </form>
                    
                    
                    
                    
            
                </div>
            </div>
            </div>
                
            
            
          
        </div>
    </div>

