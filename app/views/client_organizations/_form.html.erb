<div class="form-row row">
   <div class="col-xl-6 mt-3">
      <label for="">Organization Name</label>
      <%= f.text_field :organization_name, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6 mt-3 ">
      <label for="">Organization Type</label>
      <%= f.text_field :organization_type, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6 ">
      <label for="">Organization NPI</label>
      <%= f.text_field :organization_npi, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
</div>
<div class="form-row row">
 <h6 class="mt-3">Organization Physical Address</h6>
   <div class="col-xl-6 mt-3 ">
      <label for="">Address 1</label>
      <%= f.text_field :organization_address_1, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6 mt-3 ">
      <label for="">Address 2</label>
      <%= f.text_field :organization_address_2, class: 'form-control', placeholder: 'Type here...'%>
   </div>
</div>
<div class="form-row row">
   <div class="col-xl-6 ">
      <label for="">City</label>
      <%= f.text_field :organization_city, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6">
      <label for="">State</label>
      <div class="form-control" name="client_organization[organization_state_id]" id="dropdown-states">
      </div>
   </div>
   <div class="col-xl-6 ">
      <label for="">Zip Code</label>
      <%= f.text_field :organization_zipcode, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
</div>
<div class="form-row row">
<div class="col-xl-6 mt-3 ">
      <label for="">Phone Number</label>
      <%= f.text_field :organization_phone_number, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6 mt-3 ">
      <label for="">Fax Number</label>
      <%= f.text_field :organization_fax_number, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
</div>
<div class="form-row row">
   <div class="col-xl-6 ">
      <label for="">Organization DBA Name</label>
      <%= f.text_field :organization_dba_name, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
   <div class="col-xl-6 ">
      <label for="">Organization Tax ID</label>
      <%= f.text_field :organization_tax_id, class: 'form-control', placeholder: 'Type here...', required: true%>
   </div>
 </div>
</div>