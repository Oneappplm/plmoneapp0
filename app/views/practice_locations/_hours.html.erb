					<label for="">Comment</label>
					<%= f.text_area :comment, class: 'form-control', rows: '3' %>
	</div>
</div>
<div class="form-row row">
	<div class="col-xl-12">
					<label for="">Does this location have 24 hours/7 days per week telephone coverage?</label>
					<div class="d-flex align-items-center gap-3">
						<div class="form-check">
							<%= f.radio_button :telephone_coverage, 'yes', class: 'form-check-input', id: 'inter-yes'%>
							<label class="form-check-label" for="inter-yes">
							Yes
							</label>
						</div>
						<div class="form-check">
							<%= f.radio_button :telephone_coverage, 'no', class: 'form-check-input', id: 'inter-no'%>
							<label class="form-check-label" for="inter-no">
							No
							</label>
						</div>
      				</div>
	</div>
</div>
<div class="form-row row">
	<div class="col-xl-6">
					<label for="">Telephone Coverage Type</label>
					<%= f.select :telephone_coverage_type, options_for_select(['Answering Service', 'Voice Mail to Answering Service', 'Voice Mail Other']), { prompt: 'Select option' }, {:class => 'form-select'}  %>
	</div>
</div>
<div class="form-row row">
	<div class="col-xl-6">
					<label for="">After Hours Telephone Number</label>
					 <%= f.text_field :telephone_number_after_hours, class: 'form-control', placeholder: 'Type here...'%>
	</div>
	<div class="col-xl-6">
					<label for="">Ext</label>
					<%= f.text_field :telephone_number_ext, class: 'form-control', placeholder: 'Type here...'%>
	</div>
</div>