	<div class="col-xl-12">
		<%= HtmlUtils.radio_options name: 'practice_location[public_transportation]', label: 'Is this location accessible by public transportation?', to_show: 'transportation-wrp' %>
	</div>
</div>
<div class="form-row row" id="transportation-wrp">
	<div class="col-xl-6">
					<label for="">If Yes, please select from the following:</label>
					<%= f.select :public_transportation_wrp, options_for_select([]), { prompt: 'Select option' }, {:class => 'form-select'}  %>
	</div>
</div>
