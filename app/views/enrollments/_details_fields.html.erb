<div class="field card mb-3 p-3 py-3">
				<h5>List of Individual Ownership</h5>
				<div class="row mb-3">
					<div class="col-lg-3">
						<label class="text-dark-grey">First Name </label>
						<%= f.text_field :individual_ownership_first_name, class: 'form-control border border-dark', placeholder: 'Type here... ' %>
					</div>
					<div class="col-lg-3">
						<label class="text-dark-grey">Middle Name </label>
						<%= f.text_field :individual_ownership_middle_name, class: 'form-control border border-dark', placeholder: 'Type here... ' %>
					</div>
					<div class="col-lg-3">
						<label class="text-dark-grey">Last Name </label>
						<%= f.text_field :individual_ownership_last_name, class: 'form-control border border-dark', placeholder: 'Type here... ' %>
					</div>
					<div class="col-lg-3">
						<label class="text-dark-grey">Title </label>
						<%= f.text_field :individual_ownership_title, class: 'form-control border border-dark', placeholder: 'Type here... ' %>
					</div>
				</div>
				<div class="row mb-3">
					<div class="col-lg-4">
						<label class="text-dark-grey">SSN </label>
						<%= f.text_field :individual_ownership_ssn, class: 'form-control border border-dark', placeholder: 'xxx-xx-xxx ' %>
					</div>
					<div class="col-lg-4">
						<label class="text-dark-grey">DOB </label>
						<%= f.text_field :individual_ownership_dob, class: 'form-control border border-dark', placeholder: 'xxx-xx-xxx ' %>
					</div>
					<div class="col-lg-4">
						<label class="text-dark-grey">% of Ownership </label>
						<%= f.text_field :individual_ownership_percent_of_ownership, class: 'form-control border border-dark', placeholder: 'xxx-xx-xxx % ' %>
					</div>
				</div>
				<div class="row mb-1 mt-3">
					<div class="col-lg-6 text-start">
						<h5>Note: Add both dates if applicable.</h5>
					</div>
				</div>
				<div class="row mb-3">
					<div class="col-lg-3">
						<label class="text-dark-grey">Effective Date of Ownership: </label>
						<%= f.text_field :individual_ownership_effective_date, placeholder: 'xxx-xx-xxxx', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
					</div>
					<div class="col-lg-3">
						<label class="text-dark-grey">Acquired Managing Control Date: </label>
						<%= f.text_field :individual_ownership_control_date, placeholder: 'xxx-xx-xxxx', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
					</div>
				</div>
				<div class="row">
					<div class="col-lg-12 d-flex justify-content-end">
						<%= f.check_box :_destroy %>
						<%= f.label :_destroy, "Remove", class: 'mx-2 text-dark-grey' %>
					</div>
				</div>
				<%
=begin%>
 <hr class="divider" style="border: none; height: 0.5px; background-color: #808080; width: 95%; margin: 20px auto;"> 
<%
=end%>
			</div>