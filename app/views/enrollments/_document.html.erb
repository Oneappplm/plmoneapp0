<div id="<%= slug %>-document-holder" class="d-flex justify-content-between">
	<%= HtmlUtils.generate_view_link file_url: @enrollment_group.send(key).url, link_class: "btn btn-primary btn-sm py-1 mt-2 fw-semibold"  %>
	<% if @enrollment_group.send("#{key}?") %>
		<%= link_to 'javascript:void(0);', class: 'btn btn-sm mt-2 btn-danger py-1 text-white', "data-bs-toggle": 'modal', "data-bs-target": '#deleteFormModal', data: { enrollment_group_id: @enrollment_group.id, documentholder: "#{slug}-document-holder", key: key, title: slug.titleize } do %>
					<i class="bi bi-trash-fill "></i>
			<% end %>
	<% end %>
</div>