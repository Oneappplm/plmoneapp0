<div class="field card mb-3 p-3 py-3">
<h5>Group Contact Personnel</h5>
	<div class="row mb-2">
			<div class="col-lg-6">
        <label class="text-dark-grey">Name </label>
				<%= f.text_field :group_personnel_name, class: 'form-control border border-dark', placeholder: 'Name ' %>
			</div>
			<div class="col-lg-6">
        <label class="text-dark-grey">Email </label>
				<%= f.text_field :group_personnel_email, class: 'form-control border border-dark', placeholder: 'Email ' %>
			</div>
		</div>
		<div class="row mb-3">
			<div class="col-lg-4">
        <label class="text-dark-grey">Phone Number </label>
				<%= f.text_field :group_personnel_phone_number, class: 'form-control border border-dark', placeholder: 'Phone Number ' %>
			</div>
			<div class="col-lg-4">
        <label class="text-dark-grey">Fax Number </label>
				<%= f.text_field :group_personnel_fax_number, class: 'form-control border border-dark', placeholder: 'Fax Number ' %>
			</div>
			<div class="col-lg-4">
        <label class="text-dark-grey">Position </label>
				<%= f.text_field :group_personnel_position, class: 'form-control border border-dark', placeholder: 'Position ' %>
			</div>
	</div>	
</div>