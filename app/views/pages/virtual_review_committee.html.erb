<div class="content container-fluid">
			<div class="row align-items-center">
					<div class="col">
							<h1 class="page-header-title"><%= action_name.titleize %></h1>
					</div>
			</div>
			<div class="row">
				<div class="col-lg-7">
					<div class="card">
						<div class="card-body bg-light">
								<div class="row">
									<div class="col-lg-6">
											<form>
												<div class="form-group mb-3">
														<label for="">Provider Type</label>
														<input type="text" class="form-control" >
												</div>
												<div class="form-group mb-3">
														<label for="">Specialty</label>
														<input type="text" class="form-control">
												</div>
												<div class="row mb-3">
													<div class="col-lg-6">
														 <label for="">First Name</label>
															<input type="text" class="form-control" placeholder="">
													</div>
													<div class="col-lg-6">
														 <label for="">Last Name</label>
															<input type="text" class="form-control" placeholder="">
													</div>
												</div>
												<div class="form-group mb-3">
														<label for="">Review Level</label>
														<input type="text" class="form-control">
												</div>
												<div class="form-group mb-3">
														<label for="">Credential Cycle</label>
														<input type="text" class="form-control">
												</div>
											<div class="form-group mb-3">
													<label for="">ID Type</label>
													<input type="text" class="form-control">
											</div>
										</form>
									</div>
									<div class="col-lg-6">

										<div class="row mb-3">
											<label for="">PSV Completed</label>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="from">
											</div>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="to">
											</div>
										</div>

										<div class="row mb-3">
											<label for="">Committee Date</label>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="from">
											</div>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="to">
											</div>
										</div>

										<div class="row mb-3">
											<label for="">Review Date</label>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="from">
											</div>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="to">
											</div>
										</div>

										<div class="row mb-8">
											<label for="">State</label>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="from">
											</div>
											<div class="col-lg-6">
													<input type="text" class="form-control" placeholder="to">
											</div>
										</div>


										<button type="button" class="btn btn-primary btn-block mb-3" style="width: 100%;">Submit</button>
										<button type="button" class="btn btn-danger btn-block mb-2" style="width: 100%; background-color: red">Reset</button>

									</div>
								</div>
						</div>
					</div>
				</div>
