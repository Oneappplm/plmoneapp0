<div class="row p-4" id="page6">
	<div class="col-lg-12 text-black border border-dark py-3 px-0 pb-0">
		<%= HtmlUtils.table_section_header title: '10.2 Practice Information - Practice Location' %>
		<%= HtmlUtils.table_sub_section_header title: 'Remittance Contact' %>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Same as Office Manager/Business Office Contact:', 'Same as Billing Contact:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>
		<%= render "view_summary/components/firstname_lastname"%>
		<%= render "view_summary/components/address1_city_state"%>
		<%= render "view_summary/components/zipcode_telephone_mobile"%>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Fax Number:', 'Remittance Email Address:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>
		<%= HtmlUtils.table_sub_section_header title: 'Practice Hours' %>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Monday:', 'Tuesday:', 'Wednesday:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '4' %>
			<% end %>
		</div>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Thursday:', 'Friday:', 'Saturday:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '4' %>
			<% end %>
		</div>
		<div class="row g-0 border-bottom border-dark">
			<% titles = [['Sunday:', '4'], ['Comment:', 8]]%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title[0], height: 'h-50px', last: ((titles.count-1) == idx ), size: title[1] %>
			<% end %>
		</div>
		<div class="row g-0 border-bottom border-dark">
			<% titles = [['Ext:', '4'], ['Patient Appointment Telephone Number (if different than listed practice telephone number):', 8]]%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title[0], height: 'h-50px', last: ((titles.count-1) == idx ), size: title[1] %>
			<% end %>
		</div>
		<%= HtmlUtils.single_column title: 'Does this location have 24 hours/7 days per week telephone coverage?', height: 'h-50px'%>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Telephone Coverage Type:', 'After Hours Telephone Number:', 'Ext:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '4' %>
			<% end %>
		</div>
		<%= HtmlUtils.table_sub_section_header title: 'Patient Acceptance' %>
		<%= HtmlUtils.single_column title: 'Please select your open practice status:', height: 'h-50px'%>
		<%= HtmlUtils.single_column title: 'If patient acceptance varies by health plan, please explain:', height: 'h-75px'%>
		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Are there any practice limitations at this location?', 'Gender Limitations:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>

		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Minimum Age:', 'Maximum Age:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>
		<%= HtmlUtils.single_column title: 'Please list any other limitations:', height: 'h-75px'%>

		<%= HtmlUtils.table_sub_section_header title: 'Foreign Languages' %>
		<%= HtmlUtils.single_column title: 'Please select all language that your staffs speaks:', height: 'h-50px'%>
		<%= HtmlUtils.single_column title: 'Please select all language that your staffs writes:', height: 'h-50px'%>
		<%= HtmlUtils.single_column title: 'Are interpreters available at this location?', height: 'h-50px'%>
		<%= HtmlUtils.single_column title: 'Please select all languages interpreted:', height: 'h-50px'%>
		<%= HtmlUtils.table_sub_section_header title: 'Foreign Languages' %>


		<div class="row g-0 border-bottom border-dark">
			<% titles = ['Does this location meet ADA accessibility requirements?', 'If Yes, Please select from the following:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>

		<div class="row g-0">
			<% titles = ['Does this location offer other services for the disabled?', 'If Yes, Please select from the following:']%>
			<% titles.each_with_index do |title,idx|%>
				<%= HtmlUtils.table_column title: title, height: 'h-50px', last: ((titles.count-1) == idx ), size: '6' %>
			<% end %>
		</div>
	</div>
	<%= HtmlUtils.summary_footer page: 10 %>


</div>
<div class="row">
	<div class="col-lg-12 d-flex justify-content-end px-5 gap-3">
		<a href="javascript:void(0);" class="btn btn-primary btn-md btn-print" data-toprint="page10">Print/Download</a>
		<a href="<%= view_summary_index_path(page: 9) %>" class="btn btn-primary btn-md">Previous Page</a>
		<a href="<%= view_summary_index_path(page: 11)%>" class="btn btn-primary btn-md">Next Page</a>
	</div>
</div>