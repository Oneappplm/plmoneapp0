  <div class="row field pa-licenses-field mb-2" data-id=0>
    <div class="col-lg-6 mb-3">
      <label class="text-dark-grey">PA License Number</label>
      <%= f.text_field :pa_license_number, placeholder:'Type here...', class: 'form-control border border-dark' %>
    </div>
    <div class="col-lg-6 mb-3">
      <label class="text-dark-grey">PA Original License Issue Date</label>
      <%= f.text_field :pa_license_effective_date, placeholder: 'MM-DD-YYYY', value: Time.zone.parse(f.object.pa_license_effective_date.to_s)&.strftime('%Y-%m-%d'), class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">PA Registration State</label>
      <%= f.select :state_id, options_for_select([]), { prompt: 'State' }, {:class => 'form-select border border-dark state-select single-select',  data: { value: f.object.state_id } }  %>
    </div>
    <div class="col-lg-6 mb-3">
      <label class="text-dark-grey">PA License Renewal(Current Effective) Date</label>
      <%= f.text_field :pa_license_renewal_effective_date, value: Time.zone.parse(f.object.pa_license_renewal_effective_date.to_s)&.strftime('%Y-%m-%d'), placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-6 mb-3">
      <label class="text-dark-grey">PA License Expiration Date</label>
      <%= f.text_field :pa_license_expiration_date, value: Time.zone.parse(f.object.pa_license_expiration_date.to_s)&.strftime('%Y-%m-%d'), placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">Explain if you do not have PA License Number(required if no PA Number) </label>
      <%= f.text_field :no_pa_license, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="d-flex buttons gap-2 mt-2">
    <div class="links">
    <button type="button" class="add_association  py-1 btn btn-primary" > <i class="bi bi-plus-circle-fill"></i> Add PA License </button>
  </div>
     <% if f.object.id %>
      <button type="button" class="btn btn-danger py-1 delete-license" data-model="pa_licenses" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
    <% else %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-id=0> <i class="bi bi-trash-fill"></i></button>
    <% end %>
  </div>
  </div>