 <div class="row field medicares-field mb-2" data-id=0>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare PTAN Number (optional)</label>
      <%= f.text_field :ptan_number, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Effective Date (optional)</label>
      <%= f.text_field :effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark mb-3', onfocus: "(this.type = 'date')" %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Username (optional)</label>
      <%= f.text_field :medicare_username, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Password (optional)</label>
      <%= f.password_field :password, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Security Question (optional)</label>
      <%= f.text_field :question, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Security Answer (optional)</label>
      <%= f.text_field :answer, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
  <div class="col-lg-6">
      <label class="text-dark-grey">Medicare Reval Date (optional)</label>
      <%= f.text_field :reval_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark mb-3', onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-12">
      <label class="text-dark-grey">Medicare Notes (optional)</label>
      <%= f.text_area :notes, placeholder: 'Type here..', class: 'form-control border border-dark', rows: '3' %>
    </div>
 </div>