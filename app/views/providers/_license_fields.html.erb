 <div class="row field licenses-field mb-2">
		<div class="col-lg-3">
      <label class="text-dark-grey">License Number</label>
			<%= f.text_field :license_number, placeholder: 'Type here...', class: 'form-control border border-dark' %>
		</div>
		<div class="col-lg-4">
      <label class="text-dark-grey">License Effective Date</label>
			<%= f.text_field :license_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
		</div>
		<div class="col-lg-4">
      <label class="text-dark-grey">License Expiration Date</label>
			<%= f.text_field :license_expiration_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
		</div>
		<div class="col-lg-1">
      <label class="text-white">spacer</label>
      <% if f.object.id %>
        <button type="button" class="btn btn-danger remove-dynamic-fields w-100" data-model="licenses" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
      <% else %>
        <button type="button" class="btn btn-danger remove-dynamic-fields w-100" > <i class="bi bi-trash-fill"></i></button>
      <% end %>
		</div>
	</div>