 <div class="row field mb-2">
		<div class="col-lg-3">
      <label class="text-dark-grey">License Number *</label>
			<%= f.text_field :license_number, class: 'form-control border border-dark', placeholder: 'License Number *' %>
		</div>
		<div class="col-lg-3">
      <label class="text-dark-grey">License Effective Date *</label>
			<%= f.text_field :license_effective_date, placeholder: 'License Effective Date *', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
		</div>
		<div class="col-lg-3">
      <label class="text-dark-grey">License Expiration Date *</label>
			<%= f.text_field :license_expiration_date, placeholder: 'License Expiration Date *', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
		</div>
		<div class="col-lg-3 d-flex justify-content-center align-items-center">
			<%= f.check_box :_destroy %>
			<%= f.label :_destroy, "Remove", class: 'mx-2 text-dark-grey' %>
		</div>
	</div>