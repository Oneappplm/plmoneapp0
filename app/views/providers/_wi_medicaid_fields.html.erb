 <div class="row field mn-medicaids-field mb-2" data-id=0>
    <div class="col-lg-6">
      <label class="text-dark-grey">WI Medicaid/Forward Health (optional)</label>
      <%= f.text_field :wi_medicaid, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">WI Medicaid/Forward Health Username (optional)</label>
      <%= f.text_field :wi_medicaid_username, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">WI Medicaid Security Question (optional)</label>
      <%= f.text_field :question, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">WI Medicaid/Forward Health Password (optional)</label>
      <%= f.text_field :password, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">WI Medicaid Security Answer (optional)</label>
      <%= f.text_field :answer, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-12">
      <label class="text-dark-grey">WI Medicaid/Forward Health Notes (optional)</label>
      <%= f.text_area :notes, placeholder: 'Type here...', class: 'form-control border border-dark mb-3', rows: '3' %>
    </div>
 </div>