  <div class="row field ins-policy-field mb-2" data-id=0>
    <div class="col-lg-4">
      <label class="text-dark-grey">INS Policy Number </label>
      <%= f.text_field :ins_policy_number, placeholder:'Type here...', class: 'form-control border border-dark' %>
    </div>
    <div class="col-lg-4">
      <label class="text-dark-grey">INS Policy Effective Date </label>
      <%= f.text_field :effective_date, placeholder: 'MM/DD/YYYY', class: 'form-control border border-dark', value: display_mdy(f.object.effective_date), onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-4">
      <label class="text-dark-grey">INS Policy Expiration Date </label>
      <%= f.text_field :expiration_date, placeholder: 'MM/DD/YYYY', class: 'form-control border border-dark', value: display_mdy(f.object.expiration_date), onfocus: "(this.type = 'date')" %>
    </div>

  <div class="d-flex buttons gap-2 mt-2">
    <div class="links">
    <button type="button" class="add_association  py-1 btn btn-primary" > <i class="bi bi-plus-circle-fill"></i> INS Policy </button>
  </div>
     <% if f.object.id %>
      <button type="button" class="btn btn-danger py-1 delete-license" data-model="ins_policies" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
    <% else %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-id=0> <i class="bi bi-trash-fill"></i></button>
    <% end %>
  </div>
  </div>