 <div class="row field medicaids-field mb-2" data-id=0>
    <div class="col-lg-6">
      <label class="text-dark-grey">Medicaid Effective Date (optional)</label>
      <%= f.text_field :effective_date, placeholder: 'MM/DD/YYYY', class: 'form-control border border-dark mb-3', value: display_mdy(f.object.effective_date), onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">Medicaid Reval Date (optional)</label>
      <%= f.text_field :reval_date, placeholder: 'MM/DD/YYYY', class: 'form-control border border-dark mb-3', value: display_mdy(f.object.reval_date), onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-12">
      <label class="text-dark-grey">Medicaid Notes (optional)</label>
      <%= f.text_area :notes, placeholder: 'Type here..', class: 'form-control border border-dark', rows: '3' %>
    </div>
 </div>