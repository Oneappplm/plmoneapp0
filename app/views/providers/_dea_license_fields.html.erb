 <div class="row field dea-licenses-field mb-2" data-id=0>
    <div class="col-lg-6">
      <label class="text-dark-grey">DEA License Number(optional)</label>
      <%= f.text_field :dea_license_number, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">DEA License Effective Date(optional)</label>
      <%= f.text_field :dea_license_effective_date, placeholder: 'MM-DD-YYYY', value: Time.zone.parse(f.object.dea_license_effective_date.to_s)&.strftime('%Y-%m-%d'), class: 'form-control border border-dark mb-3', onfocus: "(this.type = 'date')" %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">DEA License Registration State(optional)</label>
      <%= f.select :state_id, options_for_select(states.pluck(:name, :id), f.object.state_id), { prompt: 'Select option' }, { class: 'form-select border-dark' } %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">DEA License Expiration Date(optional)</label>
      <%= f.text_field :dea_license_expiration_date, placeholder: 'MM-DD-YYYY', value: Time.zone.parse(f.object.dea_license_expiration_date.to_s)&.strftime('%Y-%m-%d'), class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
    </div>

  <div class="d-flex buttons gap-2 mt-2">
    <div class="links">
    <button type="button" class="add_association  py-1 btn btn-primary" > <i class="bi bi-plus-circle-fill"></i> Add DEA License </button>
  </div>
     <% if f.object.id %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-model="np_licenses" data-id="<%#= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
    <% else %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-id=0> <i class="bi bi-trash-fill"></i></button>
    <% end %>
  </div>
</div>