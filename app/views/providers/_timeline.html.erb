<div class="<%= f.status ? 'row timeline-green' : 'row timeline-red' %> g-0 comment-field rounded py-2 mb-2" data-id="<%= f.id %>">
<div class="provider-timeline col-1 col-lg-2">
		<div class="avatar-circle" style="text-align: center">
		<i class="fa fa-check" style="font-size:24px"></i>
		</div>
	</div>
	<div class="col-1 col-lg-8">
		<div class="row">
			<div class="col-lg-12 mb-2">
      <p class="mb-0 text-black"><%= f.created_at.strftime('%m/%d/%y')%></p>
			 <p class="mb-0 text-black"><strong><%= f.title %></strong></p>
        <p class="mb-0 text-black">Due date: <%= f.due_date.strftime('%m/%d/%y')%></p>
			</div>  
			<div class="col-lg-12">
				<p class="text-black"><%= f.notes %></p>
			</div>
		</div>
	</div>
	<div class="col-1 col-lg-2 remove-timeline-fields" data-id="<%= f.id %>">
    <div class="bar1"></div>
    <div class="bar2"></div>
    <div class="bar3"></div>

    <ul id="timeline-menu">
      <li>Edit</li>
      <li>Delete</li>
    </ul>
  </div>
  <div class="col-1 col-lg-2 mr-8" style=" text-align: right;">
    <button class="btn btn-manage btn-primary mark-timeline-done">Mark as done </button>
  </div>
</div>