 <div class="row field licenses-field mb-2" data-id=0>
    <div class="col-lg-6">
      <label class="text-dark-grey">MN License Number</label>
      <%= f.text_field :mn_license_number, placeholder: 'Type here...', value: display_mdy(f.object.mn_license_number), class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-6">
      <label class="text-dark-grey">MN License Expiration Date</label>
      <%= f.text_field :mn_license_expiration_date, placeholder: 'MM/DD/YYYY', value: display_mdy(f.object.mn_license_expiration_date), class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
    </div>

