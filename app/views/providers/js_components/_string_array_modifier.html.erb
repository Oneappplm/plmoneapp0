<script>
      function stringArrayModifier(str_array) {
         var array = [];
         if(str_array != '') {
          var modifiedString = str_array.replace(/&quot;/g, '"');
          var trimmedString = modifiedString.slice(1, -1);
          array = JSON.parse("[" + trimmedString + "]");
        }
        return array;
      }
</script>