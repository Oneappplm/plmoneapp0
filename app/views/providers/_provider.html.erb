<tr>
		<th scope="row"><%= provider.provider_name %></th>
		<td><%= provider.practitioner_type %></td>
		<td><%= provider.ssn %></td>
		<td><%= provider.npi %></td>
		<td><%= provider.caqhid %></td>
		<td>
			<div class="d-flex gap-2">
				<%= link_to edit_provider_path(provider), class: 'btn btn-xs btn-primary' do %>
					<i class="bi bi-pencil"></i>
				<% end %>
				<%= button_to provider_path(provider), method: :delete, data: { confirm: 'Are you sure you want to delete this provider?' }, class: 'btn btn-xs btn-danger' do %>
					<i class="bi bi-trash"></i>
				<% end %>
			</div>
		</td>
</tr>