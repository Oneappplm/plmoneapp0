  <div class="row field np-licenses-field border-dash border-1 mb-2" data-id=0>
  	<div class="col-lg-6 mb-3">
      <label class="text-dark-grey">NP License Number  <em>(if applicable, must include alpha, dashes, periods)</em></label>
  		<%= f.text_field :np_license_number, placeholder:'Type here...', class: 'form-control border border-dark' %>
  	</div>
  	<div class="col-lg-6 mb-3">
      <label class="text-dark-grey">NP Original License Issue Date</label>
  		<%= f.text_field :np_license_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', value: Time.zone.parse(f.object.np_license_effective_date.to_s)&.strftime('%Y-%m-%d'), onfocus: "(this.type = 'date')" %>
  	</div>
    <div class="col-lg-6">
      <label class="text-dark-grey">NP License State</label>
      <%= f.select :state_id, options_for_select([]), { prompt: 'State' }, {:class => 'form-select border border-dark state-select single-select',  data: { value: f.object.state_id } }  %>
    </div>
    <div class="col-lg-6 mb-3">
      <label class="text-dark-grey">NP License Renewal(Current Effective) Date</label>
  		<%= f.text_field :np_license_renewal_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', value: Time.zone.parse(f.object.np_license_renewal_effective_date.to_s)&.strftime('%Y-%m-%d'), onfocus: "(this.type = 'date')" %>
  	</div>
  	<div class="col-lg-6 mb-3">
      <label class="text-dark-grey">NP License Expiration Date</label>
  		<%= f.text_field :np_license_expiration_date, placeholder: 'MM-DD-YYYY', class: 'form-control border border-dark', value: Time.zone.parse(f.object.np_license_expiration_date.to_s)&.strftime('%Y-%m-%d'), onfocus: "(this.type = 'date')" %>
  	</div>
    <div class="col-lg-6">
      <label class="text-dark-grey">Explain if you do not have NP License Number(required if no NP Number) </label>
      <%= f.text_field :no_np_license, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>

  <div class="d-flex buttons gap-2 mt-2">
    <div class="links">
    <button type="button" class="add_association  py-1 btn btn-primary" > <i class="bi bi-plus-circle-fill"></i> Add NP License </button>
  </div>
     <% if f.object.id %>
      <button type="button" class="btn btn-danger py-1 delete-license" data-model="np_licenses" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
    <% else %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-id=0> <i class="bi bi-trash-fill"></i></button>
    <% end %>
  </div>
  </div>