<div class="row field mb-2">
	<div class="col-lg-3">
    <label class="text-dark-grey">NP License Number</label>
		<%= f.text_field :np_license_number, class: 'form-control border border-dark', placeholder: 'NP License Number' %>
	</div>
	<div class="col-lg-4">
    <label class="text-dark-grey">NP License Effective Date</label>
		<%= f.text_field :np_license_effective_date, placeholder: 'NP License Effective Date', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-4">
    <label class="text-dark-grey">NP License Expiration Date</label>
		<%= f.text_field :np_license_expiration_date, placeholder: 'NP License Expiration Date', class: 'form-control border border-dark', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-1">
      <label class="text-white">spacer</label>
      <% if f.object.id %>
        <a href="javascript:void(0);" class="btn btn-danger delete-record w-100" data-model="np_licenses" data-id="<%= f.object.id %>">
          <i class="bi bi-trash-fill"></i>
        </a>
      <% else %>
        <a href="javascript:void(0);" class="btn btn-danger remove-dynamic-fields w-100">
          <i class="bi bi-trash-fill"></i>
        </a>
      <% end %>
	</div>
</div>