<div class="row field rn-licenses-field mb-2">
	<div class="col-lg-3">
    <label class="text-dark-grey">RN License Number(optional)</label>
		<%= f.text_field :rn_license_number, placeholder: 'Type here...', class: 'form-control border-dark' %>
	</div>
	<div class="col-lg-4">
    <label class="text-dark-grey">RN License Effective Date(optional)</label>
		<%= f.text_field :rn_license_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-4">
    <label class="text-dark-grey">RN License Expiration Date(optional)</label>
		<%= f.text_field :rn_license_expiration_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-1">
      <label class="text-white">spacer</label>
      <% if f.object.id %>
        <button type="button" class="btn btn-danger remove-dynamic-fields w-100" data-model="rn_licenses" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
      <% else %>
        <button type="button" class="btn btn-danger remove-dynamic-fields w-100" > <i class="bi bi-trash-fill"></i></button>
      <% end %>
	</div>
</div>