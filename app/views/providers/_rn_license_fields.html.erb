<div class="row field rn-licenses-field mb-2" data-id=0>
	<div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN License Number</label>
		<%= f.text_field :rn_license_number, placeholder: 'Type here...', class: 'form-control border-dark' %>
	</div>
	<div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN Original License Issue Date</label>
		<%= f.text_field :rn_license_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', value: Time.zone.parse(f.object.rn_license_effective_date.to_s)&.strftime('%Y-%m-%d'), onfocus: "(this.type = 'date')" %>
	</div>
  <div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN License Renewal(Current Effective) Date</label>
		<%= f.text_field :rn_license_renewal_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', value: Time.zone.parse(f.object.rn_license_renewal_effective_date.to_s)&.strftime('%Y-%m-%d'), onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN License Expiration Date</label>
		<%= f.text_field :rn_license_expiration_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', value: Time.zone.parse(f.object.rn_license_expiration_date.to_s)&.strftime('%Y-%m-%d'),onfocus: "(this.type = 'date')" %>
	</div>
  

   <div class="d-flex buttons gap-2 mt-2">
    <div class="links">
    <button type="button" class="add_association  py-1 btn btn-primary" > <i class="bi bi-plus-circle-fill"></i> Add RN License </button>
  </div>
     <% if f.object.id %>
      <button type="button" class="btn btn-danger py-1 delete-license" data-model="np_licenses" data-id="<%= f.object.id %>" > <i class="bi bi-trash-fill"></i></button>
    <% else %>
      <button type="button" class="btn btn-danger py-1 remove-dynamic-fields" data-id=0> <i class="bi bi-trash-fill"></i></button>
    <% end %>
  </div>
</div>