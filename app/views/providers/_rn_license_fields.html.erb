<div class="row field mb-2">
	<div class="col-lg-3">
    <label class="text-dark-grey">RN License Number</label>
		<%= f.text_field :rn_license_number, class: 'form-control', placeholder: 'RN License Number' %>
	</div>
	<div class="col-lg-3">
    <label class="text-dark-grey">RN License Effective Date</label>
		<%= f.text_field :rn_license_effective_date, placeholder: 'RN License Effective Date', class: 'form-control', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-3">
    <label class="text-dark-grey">RN License Expiration Date</label>
		<%= f.text_field :rn_license_expiration_date, placeholder: 'RN License Expiration Date', class: 'form-control', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-3">
      <label class="text-white">spacer</label>
      <% if f.object.id %>
        <a href="javascript:void(0);" class="btn btn-danger delete-record w-100" data-model="rn_licenses" data-id="<%= f.object.id %>">Remove</a>
      <% else %>
        <a href="javascript:void(0);" class="btn btn-danger remove-dynamic-fields w-100">Remove</a>
      <% end %>
	</div>
</div>