<div class="row field rn-licenses-field mb-2 border-dash border-1 p-2" data-id=0>
	<div class="col-lg-6 mb-3 mt-2">
    <label class="text-dark-grey">RN License Number  <em>(if applicable, must include alpha, dashes, periods)</em></label>
		<%= f.text_field :rn_license_number, placeholder: 'Type here...', class: 'form-control border-dark' %>
	</div>
	<div class="col-lg-6 mb-3 mt-2">
    <label class="text-dark-grey">RN Original License Issue Date</label>
		<%= f.text_field :rn_license_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', value: Time.zone.parse(f.object.rn_license_effective_date.to_s)&.strftime('%m-%d-%Y'), onfocus: "(this.type = 'date')" %>
	</div>

  <div class="col-lg-6">
        <label class="text-dark-grey">RN Registration State</label>
        <%= f.select :state_id, options_for_select([]), { prompt: 'State' }, { class: 'form-select border border-dark state-select single-select', data: { value: f.object.state_id } } %>
  </div>

  <div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN License Renewal(Current Effective) Date</label>
		<%= f.text_field :rn_license_renewal_effective_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', onfocus: "(this.type = 'date')" %>
	</div>
	<div class="col-lg-6 mb-3">
    <label class="text-dark-grey">RN License Expiration Date</label>
		<%= f.text_field :rn_license_expiration_date, placeholder: 'MM-DD-YYYY', class: 'form-control border-dark', value: Time.zone.parse(f.object.rn_license_expiration_date.to_s)&.strftime('%m-%d-%Y'),onfocus: "(this.type = 'date')" %>
	</div>
  <div class="col-lg-12 pb-2 d-flex justify-content-end">
			<%= f.check_box :_destroy %>
			<%= f.label :_destroy, "Remove", class: 'mx-2 text-dark-grey' %>
		</div>
</div>