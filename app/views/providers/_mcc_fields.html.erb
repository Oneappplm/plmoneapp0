 <div class="row field licenses-field mb-2" data-id=0>
    <div class="col-lg-4">
      <label class="text-dark-grey">MCC Username (optional)</label>
      <%= f.text_field :mcc_username, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>
    <div class="col-lg-4">
      <label class="text-dark-grey">MCC Password (optional)</label>
      <%= f.password_field :password, placeholder: 'Type here...', class: 'form-control border border-dark' %>
    </div>
    <div class="col-lg-4">
      <label class="text-dark-grey">MCC Electronic Signature Code (optional)</label>
      <%= f.text_field :mcc_electronic_signature_code, placeholder: 'Type here...', class: 'form-control border border-dark mb-3' %>
    </div>

