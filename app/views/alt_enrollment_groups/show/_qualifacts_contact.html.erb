<div class="row mb-3">
   <div class="col-lg-12 card p-3">
      <div class="w-100 d-flex justify-content-between">
         <h3 class="text-primary"><%= title %></h3>
         <a href="javascript:void(0);" class="text-black fw-semibold collapsible-trigger" data-collapse="qualifacts_contacts_<%= qcontact.id %>">
         <i class="bi bi-sort-up rotate-180" style="font-size: 18px;"></i>
         </a>
      </div>
        <div class="w-100 collapsible" id="qualifacts_contacts_<%= qcontact.id %>">
            <div class="row mb-1">
              <div class="col-lg-6">
                  <label>Office/Department</label>
                  <p class="text-black fw-semibold"><%= qcontact&.department %></p>
              </div>
              <div class="col-lg-6">
                  <label>Name</label>
                  <p class="text-black fw-semibold"><%= qcontact&.name %></p>
              </div>
            </div>
            <div class="row mb-1">
              <div class="col-lg-6">
                  <label>Role</label>
                  <p class="text-black fw-semibold"><%= qcontact&.role %></p>
              </div>
              <div class="col-lg-6">
                  <label>Phone Number</label>
                  <p class="text-black fw-semibold"><%= qcontact&.phone %></p>
              </div>
            </div>
            <div class="row mb-1">
              <div class="col-lg-6">
                  <label>Email Address</label>
                  <p class="text-black fw-semibold"><%= qcontact&.email %></p>
              </div>
            </div>
        </div>
      </div>
    </div>