<div class="row mb-3">
	<div class="col-lg-6">
		<label class="form-label">Education Type*</label>
		<select class="form-select">
			<option>Graduate School</option>
			<option>Professional School</option>
			<option>Other</option>
		</select>
	</div>
	<div class="col-lg-6">
		<label class="form-label">Graduate/Professional School Location*</label>
		<select class="form-select"></select>
	</div>
</div>
<div class="row mb-3">
	<div class="col-lg-12">
		<label class="form-label">Professional School Name*</label>
		<input type="text" class="form-control" name="tf-psn">
	</div>
</div>

<div class="row mb-3">
	<div class="col-lg-6">
		<label class="form-label">Address Line 1*</label>
		<input type="text" class="form-control" name="tf-address-line1">
	</div>
