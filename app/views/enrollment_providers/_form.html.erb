		</div>
	</div>
	<div class="row mb-3">
		<div class="col-lg-12 mb-3">
			<%= f.select :status, options_for_select([
					['Submitted','submitted'],
					['Not Submitted','not_submitted'],
					['Approved','approved'],
					['Rejected','rejected']
				], @enrollment_provider.status), { prompt: 'Status of Enrollment' }, {:class => 'form-select'}  %>
		</div>
		<div class="col-lg-12 d-none enrollment_status_hidden_fields" id="enrollment_submitted">
			<%= f.text_field :application_id, placeholder: 'Application ID/Submitted ID/Tracking ID', class: 'form-control' %>
			<!-- <input type="text" name="enrollment_application_id" class="form-control" placeholder="Application ID/Submitted ID/Tracking ID" required> -->
		</div>
		<div class="col-lg-12 d-none enrollment_status_hidden_fields" id="enrollment_not_submitted">
			<%= f.text_field :not_submitted_note, placeholder: 'Note*', class: 'form-control' %>
			<!-- <input type="text" name="enrollment_not_submitted_note" class="form-control" placeholder="Note*" required> -->
		</div>
		<div class="col-lg-12 d-none enrollment_status_hidden_fields" id="enrollment_rejected">
			<%= f.text_field :not_submitted_note_rejected, placeholder: 'Reason for rejection*', class: 'form-control' %>
			<!-- <input type="text" name="enrollment_not_submitted_note" class="form-control" placeholder="Reason for rejection*" required> -->
		</div>
		<div class="col-lg-12 d-none enrollment_status_hidden_fields" id="enrollment_approved">
			<div class="row">
				<div class="col-lg-6">
					<%= f.text_field :approved_date, placeholder: 'Approved Date *', class: 'form-control', onfocus: "(this.type = 'date')" %>
					<!-- <input type="date" name="enrollment_approved_date" class="form-control" required> -->
