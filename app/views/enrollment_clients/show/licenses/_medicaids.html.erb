<h3 class="text-primary">Medicaid</h3>
<% @provider.medicaids.each do |medicaid|%>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>Medicaid Effective Date</label>
      <p class="text-black fw-semibold"><%= medicaid.effective_date&.strftime('%B %d, %Y') %></p>
   </div>
   <div class="col-lg-6">
      <label>Medicaid Reval Date</label>
      <p class="text-black fw-semibold"><%= medicaid.reval_date&.strftime('%B %d, %Y')  %></p>
   </div>
</div>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>Medicaid Notes</label>
      <p class="text-black fw-semibold"><%= medicaid.notes %></p>
   </div>
</div>
<% end %>
<% if @provider.medicaids.blank? %>
<div class="row mb-1">
   <div class="col-lg-12">
      <label>No Medicaid to show</label>
   </div>
</div>
<% end %>
