<h3 class="text-primary">Physician Assistant License</h3>
<% @provider.pa_licenses.each do |pa_license|%>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>PA License Number</label>
      <p class="text-black fw-semibold"><%= pa_license&.pa_license_number %></p>
   </div>
   <div class="col-lg-6">
      <label>PA Original License Issue Date</label>
      <p class="text-black fw-semibold"><%= pa_license&.pa_license_effective_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>PA License State</label>
      <p class="text-black fw-semibold"><%= pa_license&.state&.name %></p>
   </div>
   <div class="col-lg-6">
      <label>PA License Renewal (Current Effective) Date</label>
      <p class="text-black fw-semibold"><%= pa_license&.pa_license_renewal_effective_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>PA License Expiration Date</label>
      <p class="text-black fw-semibold"><%= pa_license&.pa_license_expiration_date&.strftime('%B %d, %Y') %></p>
   </div>
   <div class="col-lg-6">
      <label>Explain if you do not have PA License Number(required if no PA Number)</label>
      <p class="text-black fw-semibold"><%= pa_license&.no_pa_license&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<% end %>
<% if @provider.pa_licenses.blank? %>
<div class="row mb-1">
   <div class="col-lg-12">
      <label>No PA License record to show</label>
   </div>
</div>
<% end %>