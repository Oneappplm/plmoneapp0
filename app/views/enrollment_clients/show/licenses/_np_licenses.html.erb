<h3 class="text-primary">NP License</h3>
<% @provider.np_licenses.each do |np_license|%>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>PA License Number</label>
      <p class="text-black fw-semibold"><%= np_license&.np_license_number %></p>
   </div>
   <div class="col-lg-6">
      <label>PA License Effective Date</label>
      <p class="text-black fw-semibold"><%= np_license&.np_license_effective_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>Pa License Expiration Date</label>
      <p class="text-black fw-semibold"><%= np_license&.np_license_expiration_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<% end %>
<% if @provider.np_licenses.blank? %>
<div class="row mb-1">
   <div class="col-lg-12">
      <label>No NP License record to show</label>
   </div>
</div>
<% end %>