<h3 class="text-primary">INS Policy</h3>
<% @provider.ins_policies.each do |ins_policy|%>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>INS Policy Number</label>
      <p class="text-black fw-semibold"><%= ins_policy&.ins_policy_number %></p>
   </div>
   <div class="col-lg-6">
      <label>INS Policy Effective Date</label>
      <p class="text-black fw-semibold"><%= ins_policy&.effective_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<div class="row mb-1">
   <div class="col-lg-6">
      <label>INS Policy Expiration Date</label>
      <p class="text-black fw-semibold"><%= ins_policy&.expiration_date&.strftime('%B %d, %Y') %></p>
   </div>
</div>
<% end %>
<% if @provider.ins_policies.blank? %>
<div class="row mb-1">
   <div class="col-lg-12">
      <label>No INS Policy record to show</label>
   </div>
</div>
<% end %>