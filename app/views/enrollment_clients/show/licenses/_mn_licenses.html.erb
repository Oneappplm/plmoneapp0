      <h3 class="text-primary">MN License</h3>
      <% @provider.mn_licenses do |mn_license|%>
        <div class="row mb-1">
          <div class="col-lg-6">
              <label>MN License Number</label>
              <p class="text-black fw-semibold"><%= mn_license&.mn_license_number %></p>
          </div>
          <div class="col-lg-6">
              <label>MN License Expiration Date</label>
              <p class="text-black fw-semibold"><%= mn_license&.mn_license_expiration_date&.strftime('%B %d, %Y') %></p>
          </div>
        </div>
      <% end %>
      <% if @provider.mn_licenses.blank? %>
      <div class="row mb-1">
          <div class="col-lg-12">
            <label>No MN Licenses to show</label>
          </div>
        </div>
      <% end %>
