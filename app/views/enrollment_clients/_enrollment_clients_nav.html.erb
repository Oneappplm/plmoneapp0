<div class="col-xl-12 mb-3">
    <div class="d-flex justify-content-end gap-2">
    <% if !current_user.is_provider_account %>
						<% if current_user.can_access?('Ed Dashboard') %>
      	<a href="<%= dashboard_enrollment_clients_path %>" class="btn border-0 <%= action_name == 'dashboard' ? 'btn-primary text-white fw-semibold' : 'btn-secondary text-black bg-alt-grey'%>">Dashboard</a>
						<% end %>
						<% if current_user.can_access?('Groups') %>
							<a href="<%= groups_enrollment_clients_path %>" class="btn border-0 <%= ['groups', 'view_group'].include?(action_name) ? 'btn-primary text-white fw-semibold' : 'btn-secondary text-black bg-alt-grey'%>">
							Groups</a>
						<% end %>
    <% end %>
				<% if current_user.can_access?('Provider Data') %>
    	<a href="<%= enrollment_clients_path %>" class="btn border-0 <%= action_name == 'index' ? 'btn-primary text-white fw-semibold' : 'btn-secondary text-black bg-alt-grey'%>">Provider Data</a>
				<% end %>

   <% if !current_user.is_provider_account && current_user.can_access?('Reports') %>
      <a href="<%= reports_enrollment_clients_path %>" class="btn border-0 <%= action_name == 'reports' ? 'btn-primary text-white fw-semibold' : 'btn-secondary text-black bg-alt-grey'%>">Reports</a>
    <% end %>

    <% if !@incomplete_providers.blank? && current_user.can_access?('Notifications') %>
      <a href="<%= notifications_enrollment_clients_path %>" class="btn border-0 <%= action_name == 'notifications' ? 'btn-primary text-white fw-semibold' : 'btn-secondary text-black bg-alt-grey'%>">Missing Documents</a>
    <% end %>
    </div>
</div>