  <%= render "system_navs"%>


    <div class="cp-content">
        <div class="container">
            <form action="">
                <div class="row">
                    <div class="col-xl-12">
                      <%= render "data_lookup_nav"%>

                        <form action="">
                            <div class="form-row row mt-3">
                                <div class="col-xl-5">
                                    <label for="verif-group">Verification Group:</label>
                                    <select name="verif-group" id="verif-group" class="form-select">
                                        <option value=""></option>
                                    </select>
                                </div>
                                <div class="col-xl-5">
                                    <label for="item-name">Item/Scenario Name:</label>
                                    <input type="text" name="item-name" id="item-name" class="form-control" value="">
                                </div>
                            </div>
                            <div class="form-row row">
                                <div class="col-xl-5">
                                    <label for="issue">Issue:</label>
                                    <textarea name="issue" id="issue" cols="30" rows="3" class="form-control"></textarea>
                                </div>
                                <div class="col-xl-5">
                                    <label for="cred-result">Cred Result:</label>
                                    <textarea name="cred-result" id="cred-result" cols="30" rows="3" class="form-control"></textarea>
                                </div>
                            </div>
                            <div class="form-row row">
                                <div class="col-xl-5">
                                    <label for="recred-result">ReCred Result:</label>
                                    <textarea name="recred-result" id="recred-result" cols="30" rows="3" class="form-control"></textarea>
                                </div>
                            </div>
                            <div class="form-row row">
                                <div class="col-xl-5">
                                    <label for="data-source">Data Source:</label>
                                    <input type="text" name="data-source" id="data-source" class="form-control" value="">
                                </div>
                                <div class="col-xl-5">
                                    <label for="review-indicator">Review Indicator:</label>
                                    <input type="text" name="review-indicator" id="review-indicator" class="form-control" value="">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xl-10">
                                    <div class="d-flex justify-content-end gap-3 mt-3">
                                        <button type="submit" class="btn btn-primary py-1">
                                            Add
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="row mt-3">
                            <div class="col-xl-5">
                                <div class="form-control cp-search-wrp">
                                    <input type="text" class="cp-search-input" placeholder="Search">
                                    <i class='bx bx-search'></i>
                                </div>
                            </div>
                        </div>
                        <div class="row result-controls">
                            <div class="col-xl-6 filter-result">
                                <h6 class="">Result: 3 records</h6>
                            </div>
                            <div class="col-xl-6">
                                <div class="table-controls">
                                    <div class="control-wrp">
                                        <label for="limit">Show limit</label>
                                        <input type="text" id="limit" class="form-control" value="20">
                                    </div>
                                    <div class="control-wrp">
                                        <label for="page">Page 1 of 32</label>
                                        <input type="text" id="page" class="form-control" value="1">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <table class="table practice-table mt-3">
                            <thead>
                                <tr>
                                    <th  class="text-white" scope="col">Item/Scenario</th>
                                    <th class="text-white">Indicator</th>
                                    <th class="text-white">Data Source</th>
                                    <th class="text-white"></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Expired DEA or No DEA</td>
                                    <td>Exception</td>
                                    <td>SELECT * FROM [DBServer].[view_ClientPortal_DEA]</td>
                                    <td>
                                        <div class="d-flex align-items-center justify-content-end gap-3">
                                            <i class='bx bx-edit cp-blue-icon'></i>
                                            <i class='bx bx-trash cp-red-icon'></i>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>Provider Retired</td>
                                    <td>Review and Discuss</td>
                                    <td>SELECT * FROM [DBServer].[view_ClientPortal_DEA]</td>
                                    <td>
                                        <div class="d-flex align-items-center justify-content-end gap-3">
                                            <i class='bx bx-edit cp-blue-icon'></i>
                                            <i class='bx bx-trash cp-red-icon'></i>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>Provider Deceased</td>
                                    <td>Exception</td>
                                    <td>SELECT * FROM [DBServer].[view_ClientPortal_DEA]</td>
                                    <td>
                                        <div class="d-flex align-items-center justify-content-end gap-3">
                                            <i class='bx bx-edit cp-blue-icon'></i>
                                            <i class='bx bx-trash cp-red-icon'></i>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </div>