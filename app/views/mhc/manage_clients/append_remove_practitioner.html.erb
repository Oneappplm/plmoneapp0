                    <div class="mb-3">
                        <label for="middleNameNotCurrent" class="form-label">Middle Name:</label>
                        <input type="text" class="form-control" id="middleNameNotCurrent">
                    </div>
                    <div class="mb-3">
                        <label for="lastNameNotCurrent" class="form-label">Last Name:</label>
                        <input type="text" class="form-control" id="lastNameNotCurrent">
                    </div>
                    <div class="mb-3">
                        <label for="ssnNotCurrent" class="form-label">Social Security Number:</label>
                        <div class="d-flex">
                            <input type="text" class="form-control me-1" id="ssnNotCurrent1" style="width: 50px;">
