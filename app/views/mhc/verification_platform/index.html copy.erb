        <div class="container">
            <div class="row">
                <div class="col-xl-3">
                    <%= render "encompass_sidebar"%>
                </div>
                <div class="col-xl-9">
                    <%= render "encompass_nav"%>
                    <div class="row mt-3">
                        <div class="col-xl-6">
                            <div class="form-control enc-search-wrp">
                                <input type="" class="enc-search-input" placeholder="Search by Committee Date">
                                <i class='bx bx-search' ></i>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex">
                        <div class="legend-wrp mt-3">
                            <span class="enc-legend">
                                <i class='bx bxs-check-circle enc-orange-icon'></i>
