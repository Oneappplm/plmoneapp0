 <div class="enc-content">
        <div class="container">
            <%= render "top_nav_buttons"%>
            <div class="row mt-3">
                <div class="col-xl-6 d-flex align-items-center">
                    <div class="d-flex align-items-center gap-3">
                        <a href="<%= mhc_verification_platform_path(page_tab: 'ins_coverage')%>" class="link-back">
                            <i class='bx bx-left-arrow-alt'></i>
                        </a>
                        <h4 class="page-heading">Add New Insurance</h4>
                    </div>
                </div>
            </div>
            <form action="">
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label for="" class=" ">Insurance Coverage Type:</label>
                        <select name="InsuranceType" id="InsuranceType" class="form-select ">
                            <option value="">Select Option</option>
                        </select>
                    </div>
                </div>
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label for="" class=" ">Carrier:</label>
                        <input type="text" name="carrier" id="carrier" class="form-control form-control-lg"
                            placeholder="Type here..">
                    </div>
                    <div class="col-xl-6">
                        <label for="" class=" ">Policy Number:</label>
                        <input type="text" name="PolicyNumber" id="PolicyNumber" class="form-control form-control-lg"
                            placeholder="Type here..">
                    </div>
                </div>
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label class=" " for="EffectiveDate">Effective Date:</label>
                        <input type="text" id="EffectiveDate" name="EffectiveDate" class="form-control form-control-lg"
                            placeholder="MM/DD/YYYY" onfocus="(this.type='date')" onblur="(this.type='text')">
                    </div>
                    <div class="col-xl-6">
                        <label class=" " for="ExpirationDate">Expiration Date:</label>
                        <input type="text" id="ExpirationDate" name="ExpirationDate" class="form-control form-control-lg"
                            placeholder="MM/DD/YYYY" onfocus="(this.type='date')" onblur="(this.type='text')">
                    </div>
                </div>
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label for="" class=" ">Occurence Limit:</label>
                        <input type="text" name="OccurenceLimit" id="OccurenceLimit" class="form-control form-control-lg"
                            placeholder="Type here..">
                    </div>
                    <div class="col-xl-6">
                        <label for="" class=" ">Aggregate Limit:</label>
                        <input type="text" name="AggregateLimit" id="AggregateLimit" class="form-control form-control-lg"
                            placeholder="Type here..">
                    </div>
                </div>
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label for="" class=" ">Source:</label>
                        <select name="Source" id="Source" class="form-select ">
                            <option value="">Select Option</option>
                        </select>
                    </div>
                    <div class="col-xl-6">
                        <label class=" " for="SourceDate">Source Date:</label>
                        <input type="text" id="SourceDate" name="SourceDate" class="form-control form-control-lg"
                            placeholder="MM/DD/YYYY" onfocus="(this.type='date')" onblur="(this.type='text')">
                    </div>
                </div>
                <div class="form-row row">
                    <div class="col-xl-6">
                        <label for="" class=" ">Verifier:</label>
                        <input type="text" name="Verifier" id="Verifier" class="form-control form-control-lg" placeholder="Type here..">
                    </div>
                    <div class="col-xl-6">
                        <label class=" " for="VerificationDate">Verification Date:</label>
                        <input type="text" id="VerificationDate" name="VerificationDate" class="form-control form-control-lg"
                            placeholder="MM/DD/YYYY" onfocus="(this.type='date')" onblur="(this.type='text')">
                    </div>
                </div>
                <div class="form-row row mb-4">
                    <div class="col-xl-12">
                        <label for="">Review Criteria:</label>
                        <input type="text" name="ReviewCriteria" id="ReviewCriteria" class="form-control form-control-lg" placeholder="Type here..">
                    </div>
                </div>
                <div class="form-row row justify-content-end">
                    <div class="col-xl-2">
                        <button class="btn btn-primary form-control">Save</button>
                    </div>
                </div>
            </form>
        </div>
    </div>