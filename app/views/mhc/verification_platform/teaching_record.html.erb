  <div class="enc-content">
        <div class="container">
            <%= render "top_nav_buttons"%>

            <div class="add-license-content mt-4">
                <div class="row">
                    <div class="d-flex align-items-center justify-content-between">
                        <div class="d-flex align-items-center gap-3">
                            <!-- <a href="" class="link-back">
                                <i class='bx bx-left-arrow-alt' ></i>
                            </a> -->
                            <h4 class="page-heading">Teaching Appointment Record 1</h4>
                            <button type="button" class="btn btn-primary py-1" id="prac-info-btn-edit">
                                Edit
                            </button>
                            <div class="d-flex align-items-center gap-3">
                                <button type="submit" class="btn  btn-primary py-1" id="prac-info-btn-save">
                                    Save Changes
                                </button>
                                <button type="button" class="btn btn-cancel" id="prac-info-btn-cancel">
                                    Cancel
                                </button>
                            </div>
                        </div>
                        <button type="button" class="btn btn-danger page-applicable" id="">
                            <i class='bx bx-trash'></i>
                            Delete Record
                        </button>
                    </div>
                    <div class="mt-3 d-flex page-applicable align-items-center gap-2 mb-2">
                        <button class="btn btn-primary py-1 page-applicable" data-bs-toggle="modal"
                            data-bs-target="#verifLetterModal">
                            Verification Letter
                        </button>
                        <button class="btn btn-primary py-1 page-applicable">
                            Send Contact
                        </button>
                    </div>
                    <div class="mt-3 row page-applicable">
                        <div class="col-xl-12">
                            <div class="right-pane">
                                <div class="d-flex align-items-center justify-content-between">
                                    <h6 class="offset-md-5">Message Board</h6>
                                    <button class="btn btn-primary py-1" data-bs-toggle="modal"
                                        data-bs-target="#addCommentModal">
                                        Add Comment
                                    </button>
                                </div>
                                <div class="form-row mt-2">
                                    <span class="btn span-pane" data-bs-toggle="modal"
                                        data-bs-target="#deleteCommentModal">
                                        Subject: Incomplete Record
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <form action="">


                        <div class="col-xl-12 right-pane mt-3">
                            <div class="d-flex align-items-center gap-10">
                                <span class="page-note">
                                    <i class='bx bx-error-circle font-blue'></i> <span>To search for an Institution
                                        name, please click the Search button.</span>
                                </span>
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-5">
                                    <label for="supervising-prac">Institution:</label>
                                    <input type="text" class="form-control" placeholder="Type here...">
                                </div>
                                <div class="col-xl-1">
                                    <div class="d-flex align-items-center gap-10">
                                        <button class="btn-search_add_training" type="submit">
                                            Search
                                        </button>
                                    </div>
                                </div>
                                <div class="col-xl-6">
                                    <label for="training-address">Address:</label>
                                    <select name="training-address" id="training-address" class="form-select"></select>
                                </div>
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-6">
                                    <label for="training-contact">Contact:</label>
                                    <input type="text" name="training-contact" id="training-contact"
                                        placeholder="Type here..." class="form-control" required>
                                </div>
                                <div class="col-xl-6">
                                    <label for="training-add-address">Additional Address:</label>
                                    <input type="text" name="training-add-address" id="training-add-address"
                                        placeholder="Type here..." class="form-control" required>
                                </div>
                            </div>

                            <div class="form-row row mt-2">
                                <div class="col-xl-6">
                                    <label for="training-city">City:</label>
                                    <input type="text" name="training-city" id="training-city"
                                        placeholder="Type here..." class="form-control" required>
                                </div>
                                <div class="col-xl-6">
                                    <label for="training-county">County:</label>
                                    <input type="text" name="training-county" id="training-county"
                                        placeholder="Type here..." class="form-control" required>
                                </div>
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-4">
                                    <label for="training-state">State/Province:</label>
                                    <select name="training-state" id="training-state" class="form-select">
                                        <option value="">Select option</option>
                                    </select>
                                </div>
                                <div class="col-xl-4">
                                    <label for="training-zip">Zip/Postal Code:</label>
                                    <input type="text" name="training-zip" id="training-zip" placeholder="Type here..."
                                        class="form-control" required>
                                </div>
                                <div class="col-xl-4">
                                    <label for="training-country">Country:</label>
                                    <select name="training-country" id="training-country" class="form-select">
                                        <option value="">Select option</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-row row mt-2">
                                <div class="col-xl-4">
                                    <label for="training-country">Phone Number:</label>
                                    <input type="text" id="phone" name="name" placeholder="xxx-xxx-xxx"
                                        class="form-control" required>
                                </div>
                                <div class="col-xl-4">
                                    <label for="training-email">Email Address :</label>
                                    <input type="text" id="lastname" placeholder="xxx-xxx-xxx" class="form-control"
                                        required>
                                </div>
                                <div class="col-xl-4">
                                    <label for="training-fax">Fax Number:</label>
                                    <input type="text" id="training-fax" placeholder="xxx-xxx-xxx" class="form-control"
                                        required>
                                </div>
                            </div>

                        </div>
                        <div class="form-row row mt-2">
                            <div class="col-xl-6">
                                <label for="specialty">Specialty:</label>
                                <select name="specialty" id="specialty" class="form-select">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-row row">
                            <div class="col-xl-4">
                                <label for="">Program Director First Name:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                            <div class="col-xl-4">
                                <label for="">Program Director First Name:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                            <div class="col-xl-4">
                                <label for="">Program Director Practitioner Type:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="d-flex align-items-center gap-3">
                                    <label for="" class="">Is this the current Program Director?</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="prog-dir-input"
                                            id="prog-dir-yes" value="Yes">
                                        <label class="form-check-label" for="prog-dir-yes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="prog-dir-input"
                                            id="prog-dir-no" value="No">
                                        <label class="form-check-label" for="prog-dir-no">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="form-row row mt-2">
                            <div class="col-xl-4">
                                <label for="">If No, please list Current Program Director First Name:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                            <div class="col-xl-4">
                                <label for="">Current Program Director Last Name:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                            <div class="col-xl-4">
                                <label for="cur-dir-prac-type">Current Program Director Practitioner Type:</label>
                                <select name="cur-dir-prac-type" id="cur-dir-prac-type" class="form-select">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-row row">
                            <div class="col-xl-6">
                                <label for="type-appointment">Type of Appointment::</label>
                                <select name="type-appointment" id="type-appointment" class="form-select">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                            <div class="col-xl-6">
                                <label for="speciality">If other, please fill:</label>
                                <input type="text" name="" id="" placeholder="Type here..." class="form-control">
                            </div>
                        </div>
                        <div class="form-row row">
                            <div class="col-xl-5">
                                <label class=" " for="OriginalEffectiveDate">From:</label>
                                <input type="date" name="" id="" placeholder="" class="form-control">
                                <!-- <input type="text" id="OriginalEffectiveDate" name="OriginalEffectiveDate"
                                    class="form-control form-control-lg" placeholder="MM/DD/YYYY"
                                    onfocus="(this.type='date')" onblur="(this.type='text')"> -->
                                <!-- <input class="form-control form-control-lg" type="date" name="from" id="from" placeholder="From"> -->
                            </div>
                            <div class="col-xl-5">
                                <label class=" " for="expiredate">To:</label>
                                <input type="date" name="" id="" placeholder="" class="form-control">
                                <!-- <input type="text" id="expiredate" name="expiredate"
                                        class="form-control form-control-lg" placeholder="MM/DD/YYYY"
                                        onfocus="(this.type='date')" onblur="(this.type='text')"> -->
                            </div>
                            <div class="col-xl-2 d-flex align-items-end">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="present-switch">
                                    <label class=" " class="form-check-label" for="present-switch">Present</label>
                                </div>
                            </div>
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="d-flex align-items-center gap-3">
                                    <label for="" class="">Show on Tickler?</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="inter-input" id="inter-yes"
                                            value="Yes">
                                        <label class="form-check-label" for="prac-yes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="inter-input" id="inter-no"
                                            value="No">
                                        <label class="form-check-label" for="prac-no">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="form-row row mt-2">

                            <div class="col-xl-25">
                                <label for="training-com">Please List any comments</label>
                                <input type="text" name="training-com" id="training-com" placeholder="Type here..."
                                    class="form-control" required>
                            </div>

                        </div>

                        <div class="form-row mt-1 d-flex justify-content-end">
                            <button class="btn btn-training-save btn-primary" type="submit">
                                Save
                            </button>
                        </div>



                    </form>
                </div>
            </div>
        </div>
    </div>

        <!-- Add Comment Modal -->
        <div class="modal fade" id="addCommentModal" tabindex="-1" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="content">
                            <div class="modal-title">
                                <h6 class="offset-md-5">Add Comment</h6>
                                <button class="btn btn-cancel" data-bs-dismiss="modal">Cancel</button>
                            </div>
                            <form action="">
                                <div class="form-row mt-3">
                                    <label for="">Subject:</label>
                                    <input type="text" class="form-control">
                                </div>
                                <div class="form-row">
                                    <label for="">Message:</label>
                                    <textarea name="" id="" cols="30" rows="5" class="form-control"></textarea>
                                </div>
                            </form>
                            <div class="form-row mt-3 d-flex justify-content-center">
                                <button class="btn btn-primary py-1">Post Comment</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Delete Comment Modal -->
        <div class="modal fade" id="deleteCommentModal" tabindex="-1" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="content">
                            <div class="modal-title">
                                <h6 class="offset-md-5">Comment</h6>
                                <button class="btn btn-cancel" data-bs-dismiss="modal">Cancel</button>
                            </div>
                            <form action="">
                                <div class="form-row mt-3">
                                    <label for="">Subject:</label>
                                    <input type="text" class="form-control">
                                </div>
                                <div class="form-row">
                                    <label for="">Message:</label>
                                    <textarea name="" id="" cols="30" rows="5" class="form-control"></textarea>
                                </div>
                            </form>
                            <div class="form-row mt-3 d-flex justify-content-center">
                                <button class="btn btn-red">Delete Comment</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Verification Letter Modal -->
        <div class="modal fade" id="verifLetterModal" tabindex="-1" aria-labelledby="exampleModalLabel"
            aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-body">
                        <div class="content">
                            <div class="modal-title">
                                <h6 class="offset-md-3">Teaching Verification Letter</h6>
                                <button class="btn btn-cancel" data-bs-dismiss="modal">Cancel</button>
                            </div>
                            <div class="mt-3 left-pane">
                                <h6>Preview Letter</h6>
                            </div>
                            <div class="form-row row page-applicable mt-3">
                                <div class="col-xl-12">
                                    <div class="card other-name-card">
                                        <div class="card-body">
                                            <h6 class="form-heading">Send Fax</h6>
                                            <span class="page-note mt-1">
                                                <i class='bx bx-error-circle font-red'></i> <span>Please correct the
                                                    invalid fax number. Fields will be enabled once corrected.</span>
                                            </span>
                                            <div class="form-row mt-2">
                                                <label for="">Fax2Mail Email Address:</label>
                                                <input type="text" id="fax-email" name="fax-email" class="form-control"
                                                    placeholder="Type here...">
                                            </div>
                                            <div class="form-row d-flex justify-content-end mt-4">
                                                <button class="btn btn-primary py-1">
                                                    Send Fax
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-row row page-applicable mt-3">
                                <div class="col-xl-12">
                                    <div class="card other-name-card">
                                        <div class="card-body">
                                            <h6 class="form-heading">Send Email</h6>
                                            <span class="page-note mt-1">
                                                <i class='bx bx-error-circle font-red'></i> <span>Please correct the
                                                    invalid email address. Fields will be enabled once corrected.</span>
                                            </span>
                                            <div class="form-row mt-2">
                                                <label for="">Email Address To:</label>
                                                <input type="text" id="email-to" name="email-to" class="form-control"
                                                    placeholder="Type here...">
                                            </div>
                                            <div class="form-row">
                                                <label for="">Email Address CC:</label>
                                                <input type="text" id="email-cc" name="email-cc" class="form-control"
                                                    placeholder="Type here...">
                                            </div>
                                            <div class="form-row d-flex justify-content-end mt-4">
                                                <button class="btn btn-primary py-1">
                                                    Send Email
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-row mt-3 d-flex justify-content-center">
                                <button class="btn btn-red">Delete Comment</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>