		var newId = new Date().getTime();
		newPersonallyEmployedPractitioners.html(newPersonallyEmployedPractitioners.html().replace(/personally_employed_practitioners_attributes\]\[\d+\]/g, "personally_employed_practitioners_attributes][" + newId + "]"));
		$("#personally_employed_practitioners .fields").append(newPersonallyEmployedPractitioners);
