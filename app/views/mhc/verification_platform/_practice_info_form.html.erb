<script>	
	<%# js for allied health practitioners nested form %>
  $("#add-health-practitioner").click(function() {
    var alliedHealthPractitionersFields = $("#allied_health_practitioners .fields .form-row");
    var newAlliedHealthPractitioner = alliedHealthPractitionersFields.last().clone();

    // Generate a unique ID for the new form fields
    var newId = new Date().getTime();
    // Update IDs and names of the cloned fields
    newAlliedHealthPractitioner.html(
        newAlliedHealthPractitioner.html().replace(
            /allied_health_practitioners_attributes\]\[\d+\]/g,
            "allied_health_practitioners_attributes][" + newId + "]"
        )
    );
    // Reset input values and deselect radio buttons and select fields
    newAlliedHealthPractitioner.find("input").each(function() {
      if ($(this).is(":radio") || $(this).is(":checkbox")) {
        $(this).prop("checked", false); // Reset radio buttons and checkboxes
      } else {
        $(this).val(""); // Clear input values
      }
    });

    newAlliedHealthPractitioner.find("select").each(function() {
      $(this).val(""); // Clear selected option
    });
    // Update attributes for the remove button and container
    newAlliedHealthPractitioner.find('.remove-dynamic-fields').attr('data-id', newId);
    newAlliedHealthPractitioner.attr('data-id', newId);

    // Append the new form fields to the container
    $("#allied_health_practitioners .fields").append(newAlliedHealthPractitioner);
  });



	$("#allied_health_practitioners").on("click", ".remove-dynamic-fields", function() {
		var container = this.closest(".allied_health_practitioner_container");
		const containers = $(".allied_health_practitioner_container");
    
    if (containers.length > 1) {
			const destroyField = container.querySelector(".destroy-field");
	    if (destroyField) {
	      destroyField.value = "1"; // Mark for destruction
	      container.style.display = 'none' // Hide the field
	    } else {
	      container.remove(); // Remove unsaved fields
	    }
    } else {
	    alert("At least one record is required.");
	  }
	});

	<%# js for personally employed practitioners nested form %>
	$("#add-employed-practitioner").click(function() {
		var personallyEmployedPractiotionerFields = $("#personally_employed_practitioners .fields .form-row");
