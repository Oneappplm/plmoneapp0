<%= form_with(model: @practice_information, url: @url) do |f| %>
	<div class="form-row row mt-4">
		<div class="col-xl-12">
			<div class="card other-name-card">
				<div class="card-body">
					<span class="page-note">
						<%= f.hidden_field :caqh_provider_attest_id %>
						<%= f.hidden_field :provider_attest_id %>
						<%#= f.hidden_field :id, value: f.object.id %>
				  </span>
	        <div class="form-row row mt-2">
	        	<div class="col-xl-6">
	        		<div class="row">
	        			<div class="col-xl-10">
	        				<label for="facility-name">Practice/Facility Name:</label>
	        				<%= f.text_field :practice_name,  placeholder: 'Type here...', class: 'form-control border border-dark' %>
	        			</div>
	            </div>
	          </div>
	          <div class="col-xl-6">
	          	<label for="">Address:</label>
	          	<%= f.text_field :address,  placeholder: 'Type here...', class: 'form-control border border-dark' %>
	          </div>
	        </div>
	        <div class="form-row row">
	        	<div class="col-xl-6">
	        		<label for="">Suite/Mail Stop:</label>
	        		<%= f.text_field :mail_stop, id: "facility-mail-stop", placeholder:"Type here...",class:"form-control" %>
	        	</div>
	        	<div class="col-xl-6">
	        		<label for="">Additional Address:</label>
	        		<%= f.text_field :address2,  placeholder: 'Type here...', class: 'form-control border border-dark' %>
	        	</div>
	        </div>
	        <div class="form-row row">
	        	<div class="col-xl-6">
	        		<label for="">City:</label>
	        		<%= f.text_field :city,  placeholder: 'Type here...', class: 'form-control border border-dark' %>
	        	</div>
	        	<div class="col-xl-6">
	        		<label for="">County:</label>
	        		<%= f.text_field :country,  placeholder: 'Type here...', class: 'form-control border border-dark', id:"facility-country" %>
	        	</div>
	        </div>
	        <div class="form-row row">
	        	<div class="col-xl-4">
	        		<label for="facility-state">State/Province:</label>
	        		<%= f.collection_select :state, State.all, :id, :name, { include_blank: "Select State" }, class: "form-select form-select-lg" %>
	        	</div>
	        	<div class="col-xl-4">
	        		<label for="">Zip/Postal Code:</label>
	        		<%= f.text_field :zip, id:"facility-zip", type: "text", placeholder:"Type here..." , class:"form-control" %>
	        	</div>
	        	<div class="col-xl-4">
	        		<label for="facility-country">Country:</label>
	        		<%= f.text_field :country,  placeholder: 'Type here...', class: 'form-control border border-dark', id:"facility-country" %>
	        	</div>
	        </div>
	        <div class="form-row row">
	        	<div class="col-xl-4">
	        		<label for="">Office Phone Number:</label>
	        		<%= f.text_field :phone_number, id: "office-phone-number", placeholder: "Type here", class:"form-control" %>
	        	</div>
	        	<div class="col-xl-4">
	        		<label for="">Office Email Address:</label>
	        		<%= f.text_field :email_address, id: "office-email", placeholder: "Type here", class:"form-control" %>
	        	</div>
	        	<div class="col-xl-4">
	        		<label for="">Office Fax Number:</label>
	        		<%= f.text_field :fax_number, id: "office-fax", placeholder: "Type here", class:"form-control" %>
	        	</div>
	        </div>
			  </div>
			</div>
		</div>
		<div class="form-row row mt-4">
			<div class="col-xl-4">
				<label for="">Office Manager:</label>
				<%= f.text_field :office_manager, placeholder: "Type here", class:"form-control" %>
			</div>
			<div class="col-xl-4">
				<div class="row">
					<div class="col-xl-6">
						<label for="">Office Manager Phone Number:</label>
						<%= f.text_field :manager_phone_number, id:"office-manager-phone-number", type: "text", placeholder:"Type here...", class:"form-control" %>
					</div>
					<div class="col-xl-6 d-flex align-items-end">
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value=""
							id="save-office-phone-check" name="save-office-phone-check">
							<label class="form-check-label" for="save-office-phone-check">
								Save as Office Phone
							</label>
						</div>
					</div>
				</div>
			</div>
			<div class="col-xl-4">
				<div class="row">
					<div class="col-xl-6">
						<label for="">Office Manager Fax Number:</label>
						<%= f.text_field :manager_fax_number, id:"office-manager-fax", type: "text", placeholder:"Type here...", class:"form-control" %>
					</div>
					<div class="col-xl-6 d-flex align-items-end">
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" id="save-office-fax-check"
							name="save-office-fax-check">
							<label class="form-check-label" for="save-office-fax-check">
								Save as Office Fax
							</label>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="form-row row">
			<div class="col-xl-4">
				<label for="">Back Office Phone Number:</label>
				<%= f.text_field :back_office_phone_number, id:"back-office-phone-number", type: "text", placeholder:"Type here...", class:"form-control" %>
			</div>
			<div class="col-xl-4">
				<label for="type-practice">Type of Practice:</label>
				<%= f.select :type_of_practice, [["Corporation", "corporation"], ["DBA", "dba"], ["LLC", "llc"], ["Other", "other"], ["PC", "pc"]], { prompt: true }, { class:"form-select" } %>
			</div>
			<div class="col-xl-4">
				<label for="">Department Name if Hospital-Based:</label>
				<%= f.text_field :hospital_based_department_name, id:"hospital-based-dept-name", type: "text", placeholder:"Type here...", class:"form-control" %>
			</div>
		</div>
		<div class="mt-4">
			<span class="page-note">
				<i class='bx bx-error-circle font-red'></i> <span>Please check all that apply before completing
				the rest of this section.</span>
			</span>
		</div>
		<div class="form-row row mt-4">
			<div class="col-xl-6">
				<label for="practice-type">Practice Type:</label>
				<%= f.select :practice_type, [["Solo Primary Care", "solo primary care"], ["Solo Specialty Care", "solo specialty care"], ["Group Primary Care", "group primary care"], ["Group Single Specialty", "group single specialty"], ["Group Multispecialty", "group multispecialty"], ["Hospital-Based Practice", "hospital-based practice"], ["Clinic-Based Practice", "clinic-based practice"]], { prompt: true }, { class:"form-select" } %>
			</div>
			<div class="col-xl-6">
				<label for="">Group/Corporate Name as it appears on IRS W-9:</label>
				<%= f.text_field :group_name, id:"IRS-W9-group-name", name:"IRS-W9-group-name", type: "text", placeholder:"Type here...", class:"form-control" %>
			</div>
		</div>
		<div class="form-row row">
			<div class="col-xl-6">
				<label for="">Group Number Corresponding to Tax ID Number:</label>
				<%= f.text_field :group_number_with_tax_id, id:"tax-id-group-number", placeholder:"Type here...", class:"form-control" %>
			</div>
			<div class="col-xl-6">
				<label for="">Name Affiliated with Tax ID:</label>
				<%= f.text_field :name_affiliated_with_tax_id, id:"tax-id-affiliated-name", placeholder:"Type here...", class:"form-control" %>
			</div>
		</div>
		<div class="form-row row">
			<div class="col-xl-6">
				<label for="">Federal Tax ID:</label>
				<%= f.text_field :federal_tax_id, id:"federal-tax-id", placeholder:"Type here...", class:"form-control" %>
			</div>
			<div class="col-xl-6">
				<label for="">Date Joined/Expected Start Date:</label>
				<%= f.date_field :start_date, id: "affiliated-start-date", class: "form-control" %>
			</div>
		</div>
		<div class="form-row row mt-4">
			<h6 class="form-heading">Office Hours</h6>
		</div>
		<div class="">
			<span class="page-note">
				Note: Please use military standard time (24 hour clock.) Please check "CLOSED" for all days you
				do not have office hours at this location.
			</span>
		</div>
		<div class="form-row row mt-2">
			<div class="col-xl-4">
				<label for="">Monday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :mon_time_from, class: 'form-control', id: 'mon-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :mon_time_to, class: 'form-control', id: 'mon-time-to', placeholder: 'Month' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :mon_closed, class: "form-check-input", value:"", id:"mon-close-check" %>
							<label class="form-check-label" for="mon-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
			<div class="col-xl-4">
				<label for="">Tuesday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :tue_time_from, class: 'form-control', id: 'tue-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :tue_time_to, class: 'form-control', id: 'tue-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :tue_closed, class: "form-check-input", value:"", id:"tue-close-check" %>
							<label class="form-check-label" for="tue-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>

			<div class="col-xl-4">
				<label for="">Wednesday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :wed_time_from, class: 'form-control', id: 'wed-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :wed_time_to, class: 'form-control', id: 'wed-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :wed_closed, class: "form-check-input", value:"", id:"wed-close-check" %>
							<label class="form-check-label" for="wed-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="form-row row">
			<div class="col-xl-4">
				<label for="">Thurday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :thu_time_from, class: 'form-control', id: 'thu-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :thu_time_to, class: 'form-control', id: 'thu-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :thu_closed, class: "form-check-input", value:"", id:"thu-close-check" %>
							<label class="form-check-label" for="thu-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
			
			<div class="col-xl-4">
				<label for="">Friday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :fri_time_from, class: 'form-control', id: 'fri-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :fri_time_to, class: 'form-control', id: 'fri-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :fri_closed, class: "form-check-input", value:"", id:"fri-close-check" %>
							<label class="form-check-label" for="fri-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>

			<div class="col-xl-4">
				<label for="">Saturday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :sat_time_from, class: 'form-control', id: 'sat-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :sat_time_to, class: 'form-control', id: 'sat-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :sat_closed, class: "form-check-input", value:"", id:"sat-close-check" %>
							<label class="form-check-label" for="sat-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="form-row row mt-2">
			<div class="col-xl-4">
				<label for="">Sunday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :sun_time_from, class: 'form-control', id: 'sun-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :sun_time_to, class: 'form-control', id: 'sun-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :sun_closed, class: "form-check-input", value:"", id:"sun-close-check" %>
							<label class="form-check-label" for="sun-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
			
			<div class="col-xl-4">
				<label for="">Holiday</label>
				<div class="row">
					<div class="col-xl-4">
						<label for="">From:</label>
						<%= f.time_field :holiday_time_from, class: 'form-control', id: 'holiday-time-from' %>
					</div>
					<div class="col-xl-4">
						<label for="">To:</label>
						<%= f.time_field :holiday_time_to, class: 'form-control', id: 'holiday-time-to' %>
					</div>
					<div class="col-xl-4 d-flex align-items-end">
						<div class="form-check">
							<%= f.check_box :holiday_closed, class: "form-check-input", value:"", id:"holiday-close-check" %>
							<label class="form-check-label" for="holiday-close-check">
								Close
							</label>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="form-row row mt-3">
			<div class="col-xl-4">
				<label for="">Number of Patients seen weekly:</label>
				<%= f.text_field :number_of_patients_weekly, id: "weekly-patients", placeholder:"Type here...", class: "form-control" %>
			</div>
			<div class="col-xl-4">
				<label for="">Total number of Patients in your Practice:</label>
				<%= f.text_field :total_number_of_patients, id: "practice-total-patient-number", placeholder:"Type here...", class: "form-control" %>
			</div>
			<div class="col-xl-4">
				<label for="">Number of Medicare Patients:</label>
				<%= f.text_field :total_medicare_patients, id: "medicare-patient-number", placeholder:"Type here...", class: "form-control" %>
			</div>
		</div>
		<div class="form-row row">
			<div class="col-xl-4">
				<label for="">Number of Medicaid Patients:</label>
				<%= f.text_field :total_medical_medicaid_patients, id: "medicaid-patient-number", placeholder:"Type here...", class: "form-control" %>
			</div>
			<div class="col-xl-4 d-flex align-items-end gap-3">
				<div class="form-check">
					<%= f.check_box :is_ccs, { class: 'form-check-input' }, true, false %>
					<%= f.label :is_ccs, 'CCS', class: 'form-check-label' %>
				</div>
				<div class="form-check">
					<%= f.check_box :is_chdp, { class: 'form-check-input' }, true, false %>
					<%= f.label :is_chdp, 'CHDP', class: 'form-check-label' %>
				</div>
			</div>
		</div>
		<div class="form-row mt-3">
			<div class="form-check form-switch">
				<%= f.check_box :any_allied_health_practitioner, { class: "form-check-input", id:"practice-info-switch" }, true, false %>
				<%= f.label :any_allied_health_practitioner, "Do you employ any allied health
				practitioners at this location?", class:"form-check-label", for:"practice-info-switch" %>
			</div>
		</div>
		
		<%# Add the allied health practitioner %>

		<div id="allied_health_practitioners", class= "card p-2 mt-3 mb-3">
			<div class="fields field mb-3">
				<%= f.fields_for :allied_health_practitioners do |allied_health_practitioner_fields| %>
				<%= render 'mhc/verification_platform/practice_informations/allied_health_practitioners_fields', f: allied_health_practitioner_fields %>
				<% end %>
			</div>
			<div class="links mt-2 text-center">
				<button type="button" class="add_association btn btn-primary py-1" id="add-health-practitioner">Add Allied Health Practitioner</button>
			</div>
		</div>

		<div class="form-row row mt-4">
			<div class="col-xl-4">
				<label for="">What is your physician supervisor to Nurse Practitioner ratio? 1 : ?</label>
				<%= f.text_field :physician_to_nurse_ratio, placeholder: 'Type here...', class: "form-control", id: "supervisor-nurse-practitioner" %>
			</div>
			<div class="col-xl-4">
				<label for="">What is your physician supervisor to Nurse Mid-Wife ratio? 1 : ?</label>
				<%= f.text_field :physician_to_midwife_ratio, class: 'form-control', placeholder: "Type here...", id: "supervisor-nurse-mid-wife" %>
			</div>
			<div class="col-xl-4">
				<label for="">What is your physician supervisor to Physician Assistant ratio? 1 : ?</label>
				<%= f.text_field :physician_to_assistant_ratio, placeholder: 'Type here...', class: "form-control", id: "supervisor-nurse-practitioner" %>
			</div>
		</div>

		<div class="form-row mt-3">
			<div class="form-check form-switch">
				<%= f.check_box :is_employing_any_personal_practitioner, { class: "form-check-input", id:"practitioners-switch" }, true, false %>
				<%= f.label :is_employing_any_personal_practitioner, "Do you personally employ any
				practitioners at this location (do not include practitioners that are employed by the
				medical group or any allied health practitioners)?", class:"form-check-label", for:"practitioners-switch" %>
			</div>
		</div>

		<%# Add Personally Employed Practitioners %>

		<div id="personally_employed_practitioners", class= "card p-2 mt-3 mb-3">
			<div class="fields field mb-3">
				<%= f.fields_for :personally_employed_practitioners do |employed_practitioner_fields| %>
				<%= render 'mhc/verification_platform/practice_informations/personal_employed_practitioner_fields', f: employed_practitioner_fields %>
				<% end %>
			</div>
			<div class="links mt-2 text-center">
				<button type="button" class="add_association btn btn-primary py-1" id="add-employed-practitioner">Add Personally Employed Practitioners</button>
			</div>
		</div>

		<div class="form-row mt-3">
			<div class="form-check form-switch">
				<%= f.check_box :is_licensed_member_in_practice, { class: "form-check-input", id:"licensed-certified-switch" }, true, false %>
				<%= f.label :is_licensed_member_in_practice, "Are there any other licensed/certified members in your practice?", class:"form-check-label", for:"licensed-certified-switch" %>
			</div>
		</div>
		
		<%# Other Licensed / Certificate Member Fields %>
		<div id="licensed_members", class= "card p-2 mt-3 mb-3">
			<div class="fields field mb-3">
				<%= f.fields_for :licensed_members do |licensed_member_fields| %>
				<%= render 'mhc/verification_platform/practice_informations/license_member_fields', f: licensed_member_fields %>
				<% end %>
			</div>
			<div class="links text-center">
				<button type="button" class="add_association btn btn-primary py-1" id="add-licensed-members">
					Add Other Members
				</button>
			</div>
		</div>

		<div class="form-row mt-3">
			<div class="form-check form-switch">
				<%= f.check_box :is_staff_speak_any_foreign_language, { class: "form-check-input", id:"foreign-languages-switch" } %>
				<%= f.label :is_staff_speak_any_foreign_language, "Does your staff speak any foreign languages?", class:"form-check-label", for:"foreign-languages-switch" %>
			</div>
		</div>
		
		<%# Added the staff foreign languages form %>

		<div id="staff_spoken_foreign_languages", class= "card p-2 mt-3 mb-3">
			<div class="fields field mb-3">
				<%= f.fields_for :staff_spoken_foreign_languages do |staff_foreign_fields| %>
				<%= render 'mhc/verification_platform/practice_informations/staff_foreign_fields', f: staff_foreign_fields %>
				<% end %>
			</div>
			<div class="links text-center">
				<button type="button" class="add_association btn btn-primary py-1" id="add-staff-spoken-foreign-languages">
					Add Staff Language
				</button>
			</div>
		</div>

		<div class="form-row mt-3">
			<h6 class="form-heading fs-3 mt-3 mb-2">Please check services provided:</h6>
			<div class="mt-3 d-flex flex-wrap align-items-center gap-3">

				<div class="form-check">
					<%= f.check_box :radiology_services, { class: "form-check-input", id: "radiology-check" } %>
					<%= f.label :radiology_services, "Radiology Services", class: "form-check-label", for: "radiology-check" %>
				</div>

				<div class="form-check">
					<%= f.check_box :allergy_injections, { class: "form-check-input", id: "allergies-injections-check" } %>
