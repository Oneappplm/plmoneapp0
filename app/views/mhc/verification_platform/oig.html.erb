<div class="enc-content">
    <div class="container">
        <%= render "top_nav_buttons"%>
        <div class="mt-4">
            <div class="d-flex align-items-center gap-3">
                <h4 class="page-heading">Excluded/Reinstated Practitioners</h4>
                <div class="form-check form-switch">
                    <input class="form-check-input" type="checkbox">
                    <label class="form-check-label" for="disclosure-switch">Not Applicable</label>
                </div>
                <%# <a class="btn btn-primary py-1 page-applicable py-1" href="" id="EditDisclosure">
                    Edit
                </a> %>
            </div>
        </div>
        <div class="row">
            <div class="col-xl-8 row">
                <div class="col-12">
                    <label class="text-dark-grey">Practitioner Name:</label>
                    <input class="form-control border border-dark" type="text" disabled=true value="<%= @provider_personal_information.fullname %>">
                </div>
                <div class="col-12">
                    <label class="text-dark-grey">Practitioner DOB:</label>
                    <input class="form-control border border-dark" type="text" disabled=true value="<%= @provider_personal_information.birth_date&.strftime('%b %d, %Y') %>">
                </div>
                <div class="col-12">
                    <label class="text-dark-grey">Practice Address:</label>
                    <textarea name="" id="" cols="30" rows="5" class="form-control" disabled=true required>
                        <%= @provider_personal_information.practice_informations.collect { |i| "#{i.complete_address}" }.join("\n") %>
                    </textarea>
                </div>
                <div class="col-12">
                    <label class="text-dark-grey">Please list any comments:</label>
                    <textarea name="" id="" cols="30" rows="5" class="form-control" disabled=true required></textarea>
                </div>
                <div class="col-12">
                    <label class="align-self-center" for="">Show on Tickler?</label>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="tickler" value="yesTickler">
                        <label class="form-check-label" for="yesTickler">Yes</label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="tickler" value="yesTickler" checked>
                        <label class="form-check-label" for="noTickler">No</label>
                    </div>
                </div>
                <div class="col-12">
                    <table class="table table-sm" style="table-layout: fixed;">
                        <thead>
                            <tr>
                                <th class="text-white" scope="col" style="width:10%">State</th>
                                <th class="text-white" scope="col">General</th>
                                <th class="text-white" scope="col">Exclusion Type</th>
                                <th class="text-white" scope="col">Specialty</th>
                                <th class="text-white" scope="col">Date</th>
                                <th class="text-white" scope="col">Reinstatement</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% @provider_personal_information_reinstatements.each do |provider_personal_information_reinstatement| %>
                                <tr>
                                    <td><%= provider_personal_information_reinstatement.state %></td>
                                    <td><%= provider_personal_information_reinstatement.general %></td>
                                    <td><%= provider_personal_information_reinstatement.exclusion_type %></td>
                                    <td><%= provider_personal_information_reinstatement.specialty %></td>
                                    <td><%= provider_personal_information_reinstatement.process_date&.strftime('%b %d, %Y') %></td>
                                    <td><%= provider_personal_information_reinstatement.reinstatement_date&.strftime('%b %d, %Y') %></td>
                                </tr>
                            <% end %>
                        </tbody>
                    </table>
                </div>
                <div class="col-2 m-auto">
                    <a href="<%= mhc_verification_platform_path(page_tab: 'add_oig_info', provider_personal_information_id: @provider_personal_information.id) %>" class="btn btn-sm btn-primary py-1  page-applicable d-flex align-items-center gap-1">
                        <i class="bx bx-plus"></i>
                        Add
                    </a>
                </div>
            </div>
            <div class="col-xl-4 p-2 text-center">
                <div class="row">
                    <div class="col-12 m-2">
                        <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                            Verification Letter
                        </button>
                    </div>
                    <div class="col-12 m-2">
                        <button  type="button" class="btn btn-primary btn-sm" id="asdf" style="min-width: 100px; font-size: 12px" data-bs-toggle="modal" data-bs-target="#send_contact_template">
                            Send Contact
                        </button>
                    </div>
                </div>
                <div class="card other-name-card">
                    <div class="card-body">
                        <h2 class="">RVA Information</h2>
                        <div class="mt-3 row">
                            <div class="col-12 m-2">
                                <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                                    Request
                                </button>
                            </div>
                            <div class="col-12 m-2">
                                <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                                    Verification
                                </button>
                            </div>
                            <div class="col-12 m-2">
                                <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                                    Audit
                                </button>
                            </div>
                            <div class="col-12 m-2">
                                <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                                    Quality Audit
                                </button>
                            </div>
                            <div class="col-12 m-2">
                                <button class="btn btn-primary btn-sm" style="min-width: 100px; font-size: 12px" disabled>
                                    Skip RVA
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

