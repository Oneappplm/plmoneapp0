    <div class="enc-content">
        <div class="container">
            <div class="row">
                <div class="col-xl-3">
                  <%= render partial: "verification_platform/encompass_sidebar"%>
                </div>
                <div class="col-xl-9">      
                    <%= render partial: "verification_platform/encompass_nav"%>
                    <div class="mt-3 d-flex page-applicable align-items-center gap-2 mb-2">
                        <h4 class="form-heading">Advanced Filter</h4>
                    </div>
                    <div class="mt-3 left-pane">
                        <div class="form-row row">
                            <div class="col-xl-6">
                                <label for="in-select" class="">In:</label>
                                <select name="in-select" id="in-select" class="form-select  border-dark">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-row row">
                            <div class="col-xl-12">
                                <label for="" class="">Where:</label>
                                <div class="row">
                                    <div class="col-xl-4">
                                        <label for="where-opt1" class="d-none">where-opt1</label>
                                        <select name="where-opt1" id="where-opt1" class="form-select  border-dark">
                                            <option value="">Select option</option>
                                        </select>
                                    </div>
                                    <div class="col-xl-2">
                                        <label for="where-opt2" class="d-none">where-opt2</label>
                                        <select name="where-opt2" id="where-opt2" class="form-select  border-dark">
                                            <option value="">Select option</option>
                                        </select>
                                    </div>
                                    <div class="col-xl-4">
                                        <input type="text" class="form-control  border-dark" placeholder="Type here...">
                                    </div>
                                    <div class="col-xl-2">
                                        <div class="d-flex align-items-center gap-3">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="manage-practitioner-radio"
                                                    id="manage-practitioner-and" value="Yes">
                                                <label class="form-check-label" for="manage-practitioner-and">
                                                    And
                                                </label>
                                            </div>
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="manage-practitioner-radio"
                                                    id="manage-practitioner-or" value="No">
                                                <label class="form-check-label" for="manage-practitioner-or">
                                                    Or
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="d-flex gap-2 mt-2">
                            <button type="button" class="btn btn-primary py-1">
                                To Condition
                            </button>
                        </div>
                        <div class="mt-4 d-flex page-applicable align-items-center gap-2 mb-2">
                            <h4 class="form-heading">All practitioners in the client</h4>
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <label for="go-to" class="">Go to:</label>
                                <select name="go-to" id="go-to" class="form-select  border-dark">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                            <div class="col-xl-6">
                                <label for="condition-name" class="">Condition Name:</label>
                                <select name="condition-name" id="condition-name" class="form-select  border-dark">
                                    <option value="">Select option</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-row row mt-3">
                            <div class="col-xl-6">
                                <div class="form-row row">
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Display All</button>
                                    </div>
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">New</button>
                                    </div>
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Query</button>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Generate Client Report</button>
                                    </div>
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Update Affiliation Status</button>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Label Library</button>
                                    </div>
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Pick List</button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-6">
                                <div class="form-row row">
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Delete Condition</button>
                                    </div>
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Save Condition</button>
                                    </div>
                                    <div class="col-xl-4">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Report Library</button>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Update Privilege Item Status</button>
                                    </div>
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Letter Library</button>
                                    </div>
                                </div>
                                <div class="form-row row mt-2">
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Application Library</button>
                                    </div>
                                    <div class="col-xl-6">
                                        <button type="button" class="btn btn-primary py-1 btn-pane">Meeting Attendance Profile</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-xl-6">
                            <div class="form-control  border-dark enc-search-wrp">
                                <input type="text" class="enc-search-input" placeholder="Search by Committee Date">
                                <i class='bx bx-search'></i>
                            </div>
                        </div>
                    </div>
                    <div class="row result-controls">
                        <div class="col-xl-6 filter-result">
                            <h6 class="">Result: 4 records</h6>
                        </div>
                        <div class="col-xl-6">
                            <div class="table-controls">
                                <div class="control-wrp">
                                    <label for="limit">Show limit</label>
                                    <input type="text" id="limit" class="form-control  border-dark" value="20">
                                </div>
                                <div class="control-wrp">
                                    <label for="page">Page 1 of 32</label>
                                    <input type="text" id="page" class="form-control  border-dark" value="1">
                                </div>
                            </div>
                        </div>
                    </div>
                    <table class="table table-bordered practice-table mt-3">
                        <thead>
                            <tr>
                                <th  class="text-white" scope="col">Practitioner Name</th>
                                <th class="text-white">Committee Date</th>
                                <th class="text-white">Verification Status</th>
                                <th class="text-white">Practitioner SSN</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Lorem</td>
                                <td>Lorem</td>
                                <td>Lorem ipsum</td>
                                <td>Lorem ipsum</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
